package com.thinkive.fxc.ismp.bus.business.entrust.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 用户委托方式表(T_ISMP_USER_ENTRUST)模型
 * @版权: Copyright (c) 2010
 * @公司: 深圳市思迪信息技术股份有限公司
