public class AgreeNodePropertyConstants
{
	
	/**
	 * @描述:由于单个节点可能匹配多个协议，所以节点属性的key值，需要前缀来分辨这些协议对应的属性,前缀中${index}最后用添加的协议组下标替换
	 * @例子：
	 *       如果某个节点有2个协议需要签署，那么在配置协议时，节点属性key-value如下
	 *       agree|stkkh_notice|signtype = 0;
	 *       agree|stkkh_notice|submittype = 0;
	 *       agree|stkkh_notice|signtexttype = 1;
	 * @节点类型:协议签署
	 */
	public static final String PROPERTY_KEY_AGREEPREFIX = "agree";
	
	/**
	 * @描述:协议外部编号
	 * @关联数据:协议信息表(T_FXC_AGREEMENT_INFO)字段agreecode字段
	 * @节点类型:协议签署
	 */
	public static final String PROPERTY_KEY_AGREECODE = "agreecode";
	
	/**
	 * @描述:签署方式
	 * @可选值:0-中登证书，1-自建证书，2-不使用证书
	 * @节点类型:协议签署
	 */
	public static final String PROPERTY_KEY_SIGNTYPE = "signtype";
	
	/**
	 * @描述:是否提交第三方
	 * @可选值:0-否，1-是，默认值0
	 * @节点类型:协议签署
	 */
	public static final String PROPERTY_KEY_SUBMITTYPE = "submittype";
	
	/**
	 * @描述:协议原文类型
	 * @可选值:0-否，1-是，默认值0
	 * @节点类型:协议签署
	 */
	public static final String PROPERTY_KEY_SIGNTEXTTYPE = "signtexttype";
	
}
