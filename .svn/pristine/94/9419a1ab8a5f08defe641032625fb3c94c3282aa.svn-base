package com.thinkive.fxc.ismp.bus.base.basicdata.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 公共-测评问卷等级定义表(T_FXC_SURVEY_DEFINE)模型
 * @版权: Copyright (c) 2010
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 田源
 * @版本: 2.0.0 
 * @创建时间: 2016年8月18日 上午10:05:00
 */
public class SurveyDefineModel extends DataRow
{

    private static final long serialVersionUID = -902777664801508079L;
    
    private static final String ID = "ID";
    
    private static final String SUBJECT_ID = "SUBJECT_ID";
    
    private static final String UPLIMIT = "uplimit";
    
    private static final String DOWNLIMIT = "downlimit";
    
    private static final String DEFINE = "define";
    
    private static final String STATE = "state";
    
    private static final String DEFINE_VALUE = "define_value";
    
    private static final String REMARK = "remark";
    
    private static final String CREATE_DATE  = "create_date";
    
    private static final String CREATE_BY = "create_by";
    
    private static final String UPDATE_DATE = "update_date";
    
    private static final String UPDATE_BY = "update_by";
    
    /**
     * 获取自增主键
     */
    public Long getId()
    {
        return getLong(ID);
    }
    
    /**
     * 设置自增主键
     */
    public void setId(Long id)
    {
        set(ID, id);
    }
    
    /**
     * 获取问卷编号
     */
    public Long getSubjectId()
    {
        return getLong(SUBJECT_ID);
    }
    
    /**
     * 设置问卷编号
     */
    public void setSubjectId(Long subjectId)
    {
        set(SUBJECT_ID, subjectId);
    }
    
    /**
     * 获取定义分数上限值
     */
    public Long getUpLimit()
    {
        return getLong(UPLIMIT);
    }
    
    /**
     * 设置定义分数上限值
     */
    public void setUpLimit(Long upLimit)
    {
        set(UPLIMIT, upLimit);
    }
    
    /**
     * 获取定义分数下限值
     */
    public Long getDownLimit()
    {
        return getLong(DOWNLIMIT);
    }
    
    /**
     * 设置定义分数下限值
     */
    public void setDownLimit(Long downLimit)
    {
        set(DOWNLIMIT, downLimit);
    }
    
    /**
     * 获取等级名称
     */
    public String getDefine()
    {
        return getString(DEFINE);
    }
    
    /**
     * 设置等级名称
     */
    public void setDefine(String define)
    {
        set(DEFINE, define);
    }
    
    /**
     * 获取状态
     */
    public String GetState()
    {
        return getString(STATE);
    }
    
    /**
     * 设置状态
     */
    public void setState(String state)
    {
        set(STATE, state);
    }
    
    /**
     * 获取等级
     */
    public String getDefineValue()
    {
        return getString(DEFINE_VALUE);
    }
    
    /**
     * 设置等级
     */
    public void setDefineValue(String defineValue)
    {
        set(DEFINE_VALUE, defineValue);
    }
    
    /**
     * 获取备注
     */
    public String getRemark()
    {
        return getString(REMARK);
    }
    
    /**
     * 设置备注
     */
    public void setRemark(String remark)
    {
        set(REMARK, remark);
    }
    
    /**
     * 获取创建时间
     */
    public Date getCreateDate() {
        return (Date) get(CREATE_DATE);
    }

    /**
     * 设置创建时间
     */
    public void setCreateDate(Date createDate) {
        Timestamp timestamp=new Timestamp(createDate.getTime());
        set(CREATE_DATE, timestamp);
    }
    
    /**
     * 获取更新时间
     */
    public Date getUpdateDate() {
        return (Date) get(UPDATE_DATE);
    }

    /**
     * 设置更新时间
     */
    public void setUpdateDate(Date updateDate) {
        Timestamp timestamp=new Timestamp(updateDate.getTime());
        set(UPDATE_DATE, timestamp);
    }
    
    /**
     * 获取创建人
     */
    public Long getCreateBy() {
        return getLong(CREATE_BY);
    }

    /**
     * 设置创建人
     */
    public void setCreateBy(Long createBy) {
        set(CREATE_BY, createBy);
    }  
    
    /**
     * 获取修改人
     */
    public Long getUpdateBy() {
        return getLong(UPDATE_BY);
    }

    /**
     * 设置修改人
     */
    public void setUpdateBy(Long updateBy) {
        set(UPDATE_BY, updateBy);
    }  
}
