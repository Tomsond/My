package com.thinkive.fxc.ismp.bus.base.basicdata.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 公共-邮编表(T_FXC_POST)模型
 * @版权: Copyright (c) 2016 
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 田源
 * @版本: 2.0 
 * @创建时间: 2016年8月16日 下午3:51:46
 */
public class PostModel extends DataRow
{
    
    private static final long serialVersionUID = 6702715830919886101L;
    
    private static final String ID = "id";
    
    private static final String POST = "post";
    
    private static final String PROVINCE = "province";
    
    private static final String MUNICIPALITY = "municipality";
    
    private static final String DISTRICT = "district";
    
    private static final String CREATE_DATE  = "create_date";
    
    private static final String CREATE_BY = "create_by";
    
    private static final String UPDATE_DATE = "update_date";
    
    private static final String UPDATE_BY = "update_by";
    
    /**
     * 获取自增主键
     */
    public Long getId() {
        return getLong(ID);
    }

    /**
     * 设置自增主键
     */
    public void setId(Long id) {
        set(ID, id);
    }
    
    /**
     * 获取邮编
     */
    public String getPost() {
        return getString(POST);
    }

    /**
     * 设置邮编
     */
    public void setPost(String post) {
        set(POST, post);
    }
    
    /**
     * 获取省
     */
    public String getProvince() {
        return getString(PROVINCE);
    }

    /**
     * 设置省
     */
    public void setProvince(String province) {
        set(PROVINCE, province);
    }
    
    /**
     * 获取市
     */
    public String getMunicipality() {
        return getString(MUNICIPALITY);
    }

    /**
     * 设置市
     */
    public void setMunicipality(String municipality) {
        set(MUNICIPALITY, municipality);
    }
    
    /**
     * 获取区
     */
    public String getDistrict() {
        return getString(DISTRICT);
    }

    /**
     * 设置区
     */
    public void setDistrict(String district) {
        set(DISTRICT, district);
    }
    
    /**
     * 获取创建时间
     */
    public Date getCreateDate() {
       return (Date) get(CREATE_DATE);
    }

    /**
     * 设置创建时间
     */
    public void setCreateDate(Date createDate) {
        Timestamp timestamp=new Timestamp(createDate.getTime());
        set(CREATE_DATE, timestamp);
    }
    
    /**
     * 获取更新时间
     */
    public Date getUpdateDate() {
       return (Date) get(UPDATE_DATE);
    }

    /**
     * 设置更新时间
     */
    public void setUpdateDate(Date updateDate) {
        Timestamp timestamp=new Timestamp(updateDate.getTime());
        set(UPDATE_DATE, timestamp);
    }
    
    /**
     * 获取创建人
     */
    public Long getCreateBy() {
        return getLong(CREATE_BY);
    }

    /**
     * 设置创建人
     */
    public void setCreateBy(Long createBy) {
        set(CREATE_BY, createBy);
    }  
    
    /**
     * 获取修改人
     */
    public Long getUpdateBy() {
        return getLong(UPDATE_BY);
    }

    /**
     * 设置修改人
     */
    public void setUpdateBy(Long updateBy) {
        set(UPDATE_BY, updateBy);
    }
}
