package com.thinkive.fxc.ismp.bus.base.basicdata.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;
/**
 * @描述: 公共-单向视频操作流水日志(T_FXC_ONEWAYVIDEO_LOG) 模型
 * @版权: Copyright (c) 2016 
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 尹杰民
 * @版本: 2.0 
 * @创建时间: 2016年8月16日 下午3:32:13
 */
public class OnewayVideoLogModel extends DataRow
{
    
    private static final long   serialVersionUID = -8585884108627203164L;
    
    private static final String ID               = "id";
    
    private static final String BUSINESS_ID      = "business_id";
    
    private static final String LOG_TYPE         = "log_type";
    
    private static final String LOG_CONTENT      = "log_content";
    
    private static final String CREATE_DATE      = "create_date";
    
    /**
     * 获取自增主键
     */
    public Long getId()
    {
        return getLong(ID);
    }
    
    /**
     * 设置自增主键
     */
    public void setId(Long id)
    {
        set(ID, id);
    }
    
    /**
     * 获取业务流水ID
     */
    public Long getBusinessId()
    {
        return getLong(BUSINESS_ID);
    }
    
    /**
     * 设置业务流水ID
     */
    public void setBusinessId(Long businessId)
    {
        set(BUSINESS_ID, businessId);
    }
    
    /**
     * 获取日志类型
     */
    public String getLogType()
    {
        return getString(LOG_TYPE);
    }
    
    /**
     * 设置日志类型
     */
    public void setLogType(String logType)
    {
        set(LOG_TYPE, logType);
    }
    
    /**
     * 获取日志类容
     */
    public String getLogContent()
    {
        return getString(LOG_CONTENT);
    }
    
    /**
     * 设置日志类容
     */
    public void setLogContent(String logContent)
    {
        set(LOG_CONTENT, logContent);
    }
    
    /**
     * 获取创建时间
     */
    public Date getCreateDate()
    {
        return (Date) getObject(CREATE_DATE);
    }
    
    /**
     * 设置创建时间
     */
    public void setCreateDate(Date createDate)
    {
        Timestamp timestamp=new Timestamp(createDate.getTime());
        set(CREATE_DATE, timestamp);
    }
    
}
