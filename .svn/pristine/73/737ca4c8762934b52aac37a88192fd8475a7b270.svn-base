package com.thinkive.fxc.ismp.bus.base.basicdata.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 公共-银行信息表(T_FXC_BANK_INFO)模型
 * @版权: Copyright (c) 2010
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 田源
 * @版本: 2.0.0 
 * @创建时间: 2016年8月18日 上午10:23:29
 */
public class BankInfoModel extends DataRow
{
    private static final long serialVersionUID = -5454718534185542411L;
    
    private static final String ID = "id";
    
    private static final String BANK_NO = "bank_no";
    
    private static final String BANK_NAME = "bank_name";
    
    private static final String BANK_FULL_NAME = "bank_full_name";
    
    private static final String BRIEF = "brief";
    
    private static final String SMALLIMG = "smallimg";
    
    private static final String BIGIMG = "bigimg";
    
    private static final String BGIMG = "bgimg";
    
    private static final String BANK_SMALL_URL="bank_small_url";
    
    private static final String THIRD_BANK_URL="third_bank_url";
    
    private static final String ORDER_LINE = "order_line";
    
    private static final String CREATE_DATE  = "create_date";
    
    private static final String UPDATE_DATE = "update_date";

    /**
     * 获取自增主键
     */
    public Long getId()
    {
        return getLong(ID);
    }
    
    /**
     * 设置自增主键
     */
    public void setId(Long id)
    {
        set(ID, id);
    }
    
    /**
     * 获取银行编号
     */
    public String getBankNo()
    {
        return getString(BANK_NO);
    }
    
    /**
     * 设置银行编号
     */
    public void setBankNo(String bankNo)
    {
        set(BANK_NO, bankNo);
    }
    
    /**
     * 获取银行名称
     */
    public String getBankName()
    {
        return getString(BANK_NAME);
    }
    
    /**
     * 设置银行名称
     */
    public void setBankName(String bankName)
    {
        set(BANK_NAME, bankName);
    }
    
    /**
     * 获取银行全称
     */
    public String getBankFullName()
    {
        return getString(BANK_FULL_NAME);
    }
    
    /**
     * 设置银行全称
     */
    public void setBankFullName(String bankFullName)
    {
        set(BANK_FULL_NAME, bankFullName);
    }
    
    /**
     * 获取银行简介
     */
    public String getBrief()
    {
        return getString(BRIEF);
    }
    
    /**
     * 设置银行简介
     */
    public void setBrief(String brief)
    {
        set(BRIEF, brief);
    }
    
    /**
     * 获取银行小图标地址
     */
    public String getSmallImg()
    {
        return getString(SMALLIMG);
    }
    
    /**
     * 设置银行小图标地址
     */
    public void setSmallImg(String smallImg)
    {
        set(SMALLIMG, smallImg);
    }
    
    /**
     * 获取银行大图标地址
     */
    public String getBigImg()
    {
        return getString(BIGIMG);
    }
    
    /**
     * 设置银行大图标地址
     */
    public void setBigImg(String bigImg)
    {
        set(BIGIMG, bigImg);
    }
    
    /**
     * 获取图标背景地址
     */
    public String getBgImg()
    {
        return getString(BGIMG);
    }
    
    /**
     * 设置图标背景地址
     */
    public void setBgImg(String bgImg)
    {
        set(BGIMG, bgImg);
    }
    
    /**
     * 获取充值取现小图标
     */
    public String getBankSmallUrl()
    {
        return getString(BANK_SMALL_URL);
    }
    
    /**
     * 设置充值取现小图标
     */
    public void setBankSmallUrl(String bankSmallUrl)
    {
        set(BANK_SMALL_URL, bankSmallUrl);
    }
    
    /**
     * 获取三方存管显示图标地址
     */
    public String getThirdBankUrl()
    {
        return getString(THIRD_BANK_URL);
    }
    
    /**
     * 设置三方存管显示图标地址
     */
    public void setThirdBankUrl(String thirdBankUrl)
    {
        set(THIRD_BANK_URL, thirdBankUrl);
    }
    
    /**
     * 获取排序值
     */
    public Long getOrderLine() {
        return getLong(ORDER_LINE);
    }

    /**
     * 获取排序值
     */
    public void setOrderLine(Long orderLine) {
        set(ORDER_LINE, orderLine);
    }
    
    /**
     * 获取创建时间
     */
    public Date getCreateDate() {
        return (Date) get(CREATE_DATE);
    }

    /**
     * 设置创建时间
     */
    public void setCreateDate(Date createDate) {
        Timestamp timestamp = new Timestamp(createDate.getTime());
        set(CREATE_DATE, timestamp);
    }
    
    /**
     * 获取更新时间
     */
    public Date getUpdateDate() {
       return (Date) get(UPDATE_DATE);
    }

    /**
     * 设置更新时间
     */
    public void setUpdateDate(Date updateDate) {
        Timestamp timestamp = new Timestamp(updateDate.getTime());
        set(UPDATE_DATE, timestamp);
    }
    
}
