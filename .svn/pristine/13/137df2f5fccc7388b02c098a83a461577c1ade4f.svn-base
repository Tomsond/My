 * @版本: 2.0 
 * @创建时间: 2016年8月15日 上午11:03:46
 */
public class PpTaskLogModel extends DataRow
{
    
    private static final long   serialVersionUID = -8750326916265600399L;
    
    private static final String LOG_ID           = "log_id";
    
    private static final String TASK_ID          = "task_id";
    
    private static final String FLOW_ID          = "flow_id";
    
    private static final String EXEC_STATUS      = "exec_status";
    
    private static final String START_DATE       = "start_date";
    
    private static final String END_DATE         = "end_date";
    
    private static final String REMARK           = "remark";
    
    /**
     * 获取日志编号
     */
    public Long getLogId()
    {
        return getLong(LOG_ID);
    }
    
    /**
     * 设置日志编号
     */
    public void setLogId(Long logId)
    {
        set(LOG_ID, logId);
    }
    
    /**
     * 获取任务编号
     */
    public Long getTaskId()
    {
        return getLong(TASK_ID);
    }
    
    /**
     * 设置任务编号
     */
    public void setTaskId(Long taskId)
    {
        set(TASK_ID, taskId);
    }
    
    /**
     * 获取流程编号
     */
    public Long getFlowId()
    {
        return getLong(FLOW_ID);
    }
    
    /**
     * 设置流程编号
     */
    public void setFlowId(Long flowId)
    {
        set(FLOW_ID, flowId);
    }
    
    /**
     * 获取执行状态
     */
    public String getExecStatus()
    {
        return getString(EXEC_STATUS);
    }
    
    /**
     * 设置执行状态
     */
    public void setExecStatus(String execStatus)
    {
        set(EXEC_STATUS, execStatus);
    }
    
    /**
     * 获取开始时间
     */
    public Date getStartDate()
    {
       return (Date) get(START_DATE);
    }
    
    /**
     * 设置开始时间
     */
    public void setStartDate(Date startDate)
    {
        Timestamp timestamp=new Timestamp(startDate.getTime());
        set(START_DATE, timestamp);
    }
    
    /**
     * 获取结束时间
     */
    public Date getEndDate()
    {
       return (Date) get(END_DATE);
    }
    
    /**
     * 设置结束时间
     */
    public void setEndDate(Date endDate)
    {
        Timestamp timestamp=new Timestamp(endDate.getTime());
        set(END_DATE, timestamp);
    }
    
    /**
     * 获取备注
     */
    public String getRemark()
    {
        return getString(REMARK);
    }
    
    /**
     * 设置备注
     */
    public void setRemark(String remark)
    {
        set(REMARK, remark);
    }
}
