public class JjExtendDaoImpl extends ISMPBaseDao implements JjExtendDao
{
	
	@Override
	public JjExtendModel add(JjExtendModel jjExtendModel,Session session)
	{
		String id = getSeqValue(SystemConstants.DB_ISMP,JjConstants.T_ISMP_JJ_EXTEND);
		jjExtendModel.setId(Long.parseLong(id));
		jjExtendModel.setCreateDate(new Date());
		session.insert(JjConstants.T_ISMP_JJ_EXTEND, jjExtendModel);
		return jjExtendModel;
	}
}
