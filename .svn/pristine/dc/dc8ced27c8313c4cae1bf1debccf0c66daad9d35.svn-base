package com.thinkive.fxc.ismp.bus.business.resetpwd.model;

import java.sql.Timestamp;
import java.util.Date;
import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 选择重置的密码类型表(T_ISMP_RESETPWD_TYPE) 模型
