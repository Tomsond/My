package com.thinkive.fxc.ismp.bus.base.business.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 短信发送记录表(T_ISMP_SMS_USERLOG)模型
 * @版权: Copyright (c) 2010
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 刘恺
 * @版本: 2.0.0 
 * @创建时间: 2016年8月26日 下午11:39:22
 */
public class SmsSendLogModel extends DataRow
{
	
	private static final long serialVersionUID = -6890562575272609910L;
	
	private static final String ID = "id";//ID
	
	private static final String MODEL_ID = "model_id";//短信模版编号
	
	private static final String MOBILE = "mobile";//手机号码
	
	private static final String CONTENT = "content";//短信内容
	
	private static final String BUSINESS_ID = "BUSINESS_ID";//业务实例编号
	
	private static final String USER_ID = "user_id";//用户编号
	
	private static final String SEND_FLAG = "send_flag";//发送结果
	
	private static final String DESCRIPTION = "description";//描述
	
	private static final String SEND_TIME = "send_time";//发送时间
	
	private static final String BUSINESS_CODE = "business_code";//业务编号
	
	private static final String IP = "ip";//IP地址
	
	private static final String MAC = "mac";//物理地址
	
	/**
	 * 获取自增主键
	 */
	public Long getId()
	{
		return getLong(ID);
	}
	
	/**
	 * 设置自增主键
	 */
	public void setId(Long id)
	{
		set(ID, id);
	}
	
	/**
	 * 获取模版编号
	 */
	public Long getModelId()
	{
		return getLong(MODEL_ID);
	}
	
	/**
	 * 设置模版编号
	 */
	public void setModelId(Long modelId)
	{
		set(MODEL_ID, modelId);
	}
	
	/**
	 * 获取手机号
	 */
	public String getMobile()
	{
		return getString(MOBILE);
	}
	
	/**
	 * 设置手机号
	 */
	public void setMobile(String mobile)
	{
		set(MOBILE, mobile);
	}
	
	/**
	 * 获取短信内容
	 */
	public String getContent()
	{
		return getString(CONTENT);
	}
	
	/**
	 * 设置短信内容
	 */
	public void setContent(String content)
	{
		set(CONTENT, content);
	}
	
	/**
	 * 获取业务实例编号
	 */
	public String getBusinessId()
	{
		return getString(BUSINESS_ID);
	}
	
	/**
	 * 设置业务实例编号
	 */
	public void setBusinessId(String businessId)
	{
		set(BUSINESS_ID, businessId);
	}
	
	/**
	 * 获取业务实例编号
	 */
	public Long getUserId()
	{
		return getLong(USER_ID);
	}
	
	/**
	 * 设置业务实例编号
	 */
	public void setUserId(Long userId)
	{
		set(USER_ID, userId);
	}
	
	/**
	 * 获取发送结果
	 */
	public String getSendFlag()
	{
		return getString(SEND_FLAG);
	}
	
	/**
	 * 设置发送结果
	 */
	public void setSendFlag(String sendFlag)
	{
		set(SEND_FLAG, sendFlag);
	}
	
	/**
	 * 获取描述
	 */
	public String getDescription()
	{
		return getString(DESCRIPTION);
	}
	
	/**
	 * 设置描述
	 */
	public void setDescription(String description)
	{
		set(DESCRIPTION, description);
	}
	
	/**
	 * 获取发送时间
	 */
	public Date getSendTime()
	{
		return (Date) get(SEND_TIME);
	}
	
	/**
	 * 设置发送时间
	 */
	public void setSendTime(Date sendTime)
	{
		Timestamp timestamp = new Timestamp(sendTime.getTime());
		set(SEND_TIME, timestamp);
	}
	
	public String getBusinessCode()
	{
		return getString(BUSINESS_CODE);
	}
	
	public void setBusinessCode(String businessCode)
	{
		set(BUSINESS_CODE, businessCode);
	}
	
	public String getIp()
	{
		return getString(IP);
	}
	
	public void setIp(String ip)
	{
		set(IP, ip);
	}
	
	public String getMac()
	{
		return getString(MAC);
	}
	
	public void setMac(String mac)
	{
		set(MAC, mac);
	}
	
}
