package com.thinkive.fxc.ismp.bus.base.basicdata.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 公共-三方存管银行信息(T_FXC_THIRD_BANK)模型
 * @版权: Copyright (c) 2010
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 田源
 * @版本: 2.0.0 
 * @创建时间: 2016年8月26日 下午4:15:28
 */
public class ThirdBankModel extends DataRow
{

    private static final long serialVersionUID = 6317138185275884876L;
    
    private static final String ID="id";
    
    private static final String BANK_NO="bank_no";
    
    private static final String BIND_TYPE="bind_type";
    
    private static final String IS_PWD="is_pwd";
    
    private static final String IS_BUFFET="is_buffet";
    
    private static final String SIGN_TYPE="sign_type";
    
    private static final String SIGN_INFO="sign_info";
    
    private static final String IS_CARD="is_card";
    
    private static final String CREATE_DATE="create_date";
    
    private static final String UPDATE_DATE="update_date";
    
    private static final String AGREE_NO="agree_no";
    
    private static final String LINK_BANK_ORG="link_bank_org";
    
    /**
     * 获取自增主键
     */
    public Long getId() {
        return getLong(ID);
    }

    /**
     * 设置自增主键
     */
    public void setId(Long id) {
        set(ID, id);
    }
    
    /**
     * 获取银行编号
     */
    public String getBankNo() {
        return getString(BANK_NO);
    }

    /**
     * 设置银行编号
     */
    public void setBankNo(String bankNo) {
        set(BANK_NO, bankNo);
    }
    
    /**
     * 获取绑定方式 (1：一步式 2：预指定)
     */
    public String getBindType() {
        return getString(BIND_TYPE);
    }

    /**
     * 设置绑定方式 (1：一步式 2：预指定)
     */
    public void setBindType(String bindType) {
        set(BIND_TYPE, bindType);
    }
    
    /**
     * 获取一步式绑定是否需要密码(1需要，0不需要)
     */
    public String getIsPwd() {
        return getString(IS_PWD);
    }

    /**
     * 设置一步式绑定是否需要密码(1需要，0不需要)
     */
    public void setIsPwd(String isPwd) {
        set(IS_PWD, isPwd);
    }
    
    /**
     * 获取是否支持网上开户(1：支持 0:不支持)
     */
    public String getIsBuffet() {
        return getString(IS_BUFFET);
    }

    /**
     * 设置是否支持网上开户(1：支持 0:不支持)
     */
    public void setIsBuffet(String isBuffet) {
        set(IS_BUFFET, isBuffet);
    }
    
    /**
     * 获取是否支持一卡多签(1：支持 0:不支持)
     */
    public String getSignType() {
        return getString(SIGN_TYPE);
    }

    /**
     * 设置是否支持一卡多签(1：支持 0:不支持)
     */
    public void setSignType(String signType) {
        set(SIGN_TYPE, signType);
    }
    
    /**
     * 获取提示信息
     */
    public String getSignInfo() {
        return getString(SIGN_INFO);
    }

    /**
     * 设置提示信息
     */
    public void setSignInfo(String signInfo) {
        set(SIGN_INFO, signInfo);
    }
    
    /**
     * 获取预指定是否需要输入银行卡号 1或者空:需要 0不需要
     */
    public String getIsCard() {
        return getString(IS_CARD);
    }

    /**
     * 设置预指定是否需要输入银行卡号 1或者空:需要 0不需要
     */
    public void setIsCard(String isCard) {
        set(IS_CARD, isCard);
    }
    
    /**
     * 获取创建时间
     */
    public Date getCreateDate()
    {
       return (Date) get(CREATE_DATE);
    }
    
    /**
     * 设置创建时间
     */
    public void setCreateDate(Date createDate)
    {
        Timestamp timestamp = new Timestamp(createDate.getTime());
        set(CREATE_DATE, timestamp);
    }
    
    /**
     * 获取更新时间
     */
    public Date getUpdateDate()
    {
        return (Date) get(UPDATE_DATE);
    }
    
    /**
     * 设置更新时间
     */
    public void setUpdateDate(Date updateDate)
    {
        Timestamp timestamp = new Timestamp(updateDate.getTime());
        set(UPDATE_DATE, timestamp);
    }
    
    /**
     * 获取协议编号
     */
    public String getAgreeNo() {
        return getString(AGREE_NO);
    }

    /**
     * 设置协议编号
     */
    public void setAgreeNo(String agreeNo) {
        set(AGREE_NO, agreeNo);
    }
    
    /**
     * 获取账户系统银行机构号
     */
    public String getLinkBankOrg() {
        return getString(LINK_BANK_ORG);
    }

    /**
     * 设置账户系统银行机构号
     */
    public void setLinkBankOrg(String linkBankOrg) {
        set(LINK_BANK_ORG, linkBankOrg);
    }
}
