<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:util="http://www.springframework.org/schema/util" xmlns:jdbc="http://www.springframework.org/schema/jdbc"
	xmlns:cache="http://www.springframework.org/schema/cache"
	xsi:schemaLocation="http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
    http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx.xsd
    http://www.springframework.org/schema/jdbc http://www.springframework.org/schema/jdbc/spring-jdbc.xsd
    http://www.springframework.org/schema/cache http://www.springframework.org/schema/cache/spring-cache.xsd
    http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd
    http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">

	<!-- <bean id="sessionAspect" class="util.SessionAspect" /> <bean id="serverDemo" 
		class="service.ServerDemo" /> <aop:config> <aop:aspect ref="sessionAspect"> 
		<aop:around method="around" pointcut-ref="execution(* update*(..))"/> </aop:aspect> 
		</aop:config> -->

	<!-- 节点属性配置 -->
	<bean id="nodePropertyConfigDao"
		class="com.thinkive.fxc.ismp.bus.base.business.dao.impl.NodePropertyConfigDaoImpl" />
	<bean id="nodePropertyConfigService"
		class="com.thinkive.fxc.ismp.bus.base.business.service.impl.NodePropertyConfigServiceImpl">
		<property name="nodePropertyConfigDao" ref="nodePropertyConfigDao" />
	</bean>

	<!-- 业务实例 -->
	<bean id="businessEntityDao"
		class="com.thinkive.fxc.ismp.bus.base.business.dao.impl.BusinessEntityDaoImpl" />
	<bean id="businessEntityService"
		class="com.thinkive.fxc.ismp.bus.base.business.service.impl.BusinessEntityServiceImpl">
		<property name="businessEntityDao" ref="businessEntityDao" />
		<property name="flowNodeDao" ref="flowNodeDao" />
	</bean>

	<!-- 业务流程节点配置 -->
	<bean id="bprocNodeConfigDao"
		class="com.thinkive.fxc.ismp.bus.base.business.dao.impl.BprocNodeConfigDaoImpl" />
	<bean id="bprocNodeConfigService"
		class="com.thinkive.fxc.ismp.bus.base.business.service.impl.BprocNodeConfigServiceImpl">
		<property name="bprocNodeConfigDao" ref="bprocNodeConfigDao" />
	</bean>

	<!-- 业务办理流程节点记录 -->
	<bean id="flowNodeDao"
		class="com.thinkive.fxc.ismp.bus.base.business.dao.impl.FlowNodeDaoImpl" />
	<bean id="flowNodeService"
		class="com.thinkive.fxc.ismp.bus.base.business.service.impl.FlowNodeServiceImpl">
		<property name="flowNodeDao" ref="flowNodeDao" />
	</bean>

	<!-- 业务办理节点操作记录 -->
	<bean id="nodeLogDao"
		class="com.thinkive.fxc.ismp.bus.base.business.dao.impl.NodeLogDaoImpl" />

	<!-- 公共问卷 -->
	<bean id="surveyAnswerDao"
		class="com.thinkive.fxc.ismp.bus.base.basicdata.dao.impl.SurveyAnswerDaoImpl" />
	<bean id="surveyDefineDao"
		class="com.thinkive.fxc.ismp.bus.base.basicdata.dao.impl.SurveyDefineDaoImpl" />
	<bean id="surveyService"
		class="com.thinkive.fxc.ismp.bus.base.basicdata.service.impl.SurveyServiceImpl">
		<property name="surveyAnswerDao" ref="surveyAnswerDao" />
		<property name="surveyDefineDao" ref="surveyDefineDao" />
	</bean>

	<!-- 公共银行 -->
	<bean id="bankInfoDao"
		class="com.thinkive.fxc.ismp.bus.base.basicdata.dao.impl.BankInfoDaoImpl" />
	<bean id="bankService"
		class="com.thinkive.fxc.ismp.bus.base.basicdata.service.impl.BankServiceImpl">
		<property name="bankInfoDao" ref="bankInfoDao" />
	</bean>

	<!-- 公共营业部 -->
	<bean id="branchExtinfoDao"
		class="com.thinkive.fxc.ismp.bus.base.basicdata.dao.impl.BranchExtinfoDaoImpl" />
	<bean id="branchInfoService"
		class="com.thinkive.fxc.ismp.bus.base.basicdata.service.impl.BranchInfoServiceImpl">
		<property name="branchExtinfoDao" ref="branchExtinfoDao" />
	</bean>

	<!-- 用户信息 -->
	<bean id="userInfoDao"
		class="com.thinkive.fxc.ismp.bus.base.business.dao.impl.UserInfoDaoImpl" />
	<bean id="userInfoService"
		class="com.thinkive.fxc.ismp.bus.base.business.service.impl.UserInfoServiceImpl">
		<property name="userInfoDao" ref="userInfoDao" />
	</bean>
	
	<!-- 用户登录日志 -->
	<bean id="userloginLogDao"
		class="com.thinkive.fxc.ismp.bus.base.business.dao.impl.UserLoginLogDaoImpl" />
	<bean id="userLoginLogService"
		class="com.thinkive.fxc.ismp.bus.base.business.service.impl.UserLoginLogServiceImpl">
		<property name="userloginLogDao" ref="userloginLogDao" />
	</bean>
</beans>