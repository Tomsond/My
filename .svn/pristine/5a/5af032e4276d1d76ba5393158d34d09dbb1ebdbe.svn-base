package com.thinkive.fxc.ismp.bus.base.basicdata.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 公共-城市信息表(T_FXC_CITY)模型
 * @版权: Copyright (c) 2016 
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 田源
 * @版本: 2.0 
 * @创建时间: 2016年8月16日 下午3:35:59
 */
public class CityModel extends DataRow
{

    private static final long serialVersionUID = -4565032123159941268L;
    
    private static final String ID = "id";
    
    private static final String REGION_ID = "region_id";
    
    private static final String CITY_NAME = "city_name";
    
    private static final String PROVINCE_NO = "province_no";
    
    private static final String ORDER_NUM = "order_num";
    
    private static final String IS_DISPLAY = "is_display";
    
    private static final String CREATE_DATE  = "create_date";
    
    private static final String CREATE_BY = "create_by";
    
    private static final String UPDATE_DATE = "update_date";
    
    private static final String UPDATE_BY = "update_by";
    
    /**
     * 获取自增主键
     */
    public Long getId() {
        return getLong(ID);
    }

    /**
     * 设置自增主键
     */
    public void setId(Long id) {
        set(ID, id);
    }
    
    /**
     * 获取所属区域id
     */
    public Long getRegionId() {
        return getLong(REGION_ID);
    }

    /**
     * 设置所属区域id 
     */
    public void setRegionId(Long regionId) {
        set(REGION_ID, regionId);
    }
    
    /**
     * 获取城市名称
     */
    public String getCityName() {
        return getString(CITY_NAME);
    }

    /**
     * 设置城市名称
     */
    public void setCityName(String cityName) {
        set(CITY_NAME, cityName);
    }
    
    /**
     * 获取省份编号
     */
    public Long getProvinceNo() {
        return getLong(PROVINCE_NO);
    }

    /**
     * 设置省份编号
     */
    public void setProvinceNo(Long provinceNo) {
        set(PROVINCE_NO, provinceNo);
    }
    
    /**
     * 获取排序
     */
    public Long getOrderNum() {
        return getLong(ORDER_NUM);
    }

    /**
     * 设置排序
     */
    public void setOrderNum(Long orderNum) {
        set(ORDER_NUM, orderNum);
    }
    
    /**
     * 获取是否显示
     */
    public Integer getIsDisplay() {
        return getInt(IS_DISPLAY);
    }

    /**
     * 设置是否显示
     */
    public void setIsDisplay(Integer isDisplay) {
        set(IS_DISPLAY, isDisplay);
    }
    
    /**
     * 获取创建时间
     */
    public Date getCreateDate() {
       return (Date) get(CREATE_DATE);
    }

    /**
     * 设置创建时间
     */
    public void setCreateDate(Date createDate) {
        Timestamp timestamp=new Timestamp(createDate.getTime());
        set(CREATE_DATE, timestamp);
    }
    
    /**
     * 获取更新时间
     */
    public Date getUpdateDate() {
       return (Date) get(UPDATE_DATE);
    }

    /**
     * 设置更新时间
     */
    public void setUpdateDate(Date updateDate) {
        Timestamp timestamp=new Timestamp(updateDate.getTime());
        set(UPDATE_DATE, timestamp);
    }
    
    /**
     * 获取创建人
     */
    public Long getCreateBy() {
        return getLong(CREATE_BY);
    }

    /**
     * 设置创建人
     */
    public void setCreateBy(Long createBy) {
        set(CREATE_BY, createBy);
    }  
    
    /**
     * 获取修改人
     */
    public Long getUpdateBy() {
        return getLong(UPDATE_BY);
    }

    /**
     * 设置修改人
     */
    public void setUpdateBy(Long updateBy) {
        set(UPDATE_BY, updateBy);
    }  
}
