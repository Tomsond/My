public class ZkServiceImpl implements ZkService
{
	
	private ZkExtendDao zkExtendDao;
	
	public void setZkExtendDao(ZkExtendDao zkExtendDao)
	{
		this.zkExtendDao = zkExtendDao;
	}
	
	@Override
	public void addOrUpdateZkExtendModel(ZkExtendModel zkExtendModel)
	{
		int count = zkExtendDao.update(zkExtendModel);
		if (0 == count)
		{
			zkExtendDao.add(zkExtendModel);
		}
	}
}
