package com.thinkive.fxc.ismp.bus.base.business.vo;

import java.util.Date;

import org.apache.commons.lang.StringUtils;

import com.thinkive.base.jdbc.DataRow;
import com.thinkive.base.util.DateHelper;

/**
 * @描述: 业务流程节点配置表(T_ISMP_BPROCNODE_CONFIG)和业务流程节点定义表(T_ISMP_PROCNODE_DEFINE)
 * @版权: Copyright (c) 2016 
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 刘恺
 * @版本: 2.0 
 * @创建时间: 2016年8月12日 下午1:45:33
 */
public class BprocNodeConfigAndDefineVO extends DataRow
{
	
	private static final long serialVersionUID = -3325693834409921760L;
	
	/**
	 * 业务流程节点配置表(T_ISMP_BPROCNODE_CONFIG)
	 */
	private static final String ID = "id";
	
	private static final String BTYPE_ID = "btype_id";
	
	private static final String NODE_ID = "node_id";
	
	private static final String BPNODE_POSITION = "bpnode_position";
	
	private static final String PRE_BPNODE_ID = "pre_bpnode_id";
	
	private static final String NEXT_BPNODE_ID = "next_bpnode_id";
	
	private static final String NEED_COMPLETE_NODE = "need_complete_node";
	
	private static final String IS_CONFIRM_ALL = "is_confirm_all";
	
	private static final String CREATE_DATE = "create_date";
	
	private static final String UPDATE_DATE = "update_date";
	
	private static final String CREATE_USER = "create_user";
	
	private static final String LAST_UPDATEUSER = "last_updateuser";
	
	/**
	 * 节点定义表（T_ISMP_PROCNODE_DEFINE）
	 */
	private static final String PAGE_ID = "page_id";//节点对应H5的pageid标识
	
	private static final String FUNC_NO = "func_no";//节点对应资料提交Function功能号
	
	private static final String NODE_TYPE = "node_type";//关联节点属性类型，例如：身份证上传、协议签署、视频、问卷等
	
	/**
	 * 获取自增主键
	 */
	public Long getId()
	{
		return getLong(ID);
	}
	
	/**
	 * 设置自增主键
	 */
	public void setId(Long id)
	{
		set(ID, id);
	}
	
	/**
	 * 获取流程标识
	 */
	public Long getBtypeId()
	{
		return getLong(BTYPE_ID);
	}
	
	/**
	 * 设置流程标识
	 */
	public void setBtypeId(Long btypeId)
	{
		set(BTYPE_ID, btypeId);
	}
	
	/**
	 * 获取节点英文标识
	 */
	public String getNodeId()
	{
		return getString(NODE_ID);
	}
	
	/**
	 * 设置节点英文标识
	 */
	public void setNodeId(String nodeId)
	{
		set(NODE_ID, nodeId);
	}
	
	/**
	 * 获取业务流程位置编号
	 */
	public int getBpNodePosition()
	{
		return getInt(BPNODE_POSITION);
	}
	
	/**
	 * 设置业务流程位置编号
	 */
	public void setBpNodePosition(int bpNodePosition)
	{
		set(BPNODE_POSITION, bpNodePosition);
	}
	
	/**
	 * 获取上一节点英文标识
	 */
	public String getPreBpNodeId()
	{
		return getString(PRE_BPNODE_ID);
	}
	
	/**
	 * 设置上一节点英文标识
	 */
	public void setPreBpNodeId(String preBpNodeId)
	{
		set(PRE_BPNODE_ID, preBpNodeId);
	}
	
	/**
	 * 获取下一节点英文标识
	 */
	public String getNextBpNodeId()
	{
		return getString(NEXT_BPNODE_ID);
	}
	
	/**
	 * 设置下一节点英文标识
	 */
	public void setNextBpNodeId(String nextBpNodeId)
	{
		set(NEXT_BPNODE_ID, nextBpNodeId);
	}
	
	/**
	 * 获取依赖步骤英文标识串
	 */
	public String getNeedCompleteNode()
	{
		return getString(NEED_COMPLETE_NODE);
	}
	
	/**
	 * 设置依赖步骤英文标识串
	 */
	public void setNeedCompleteNode(String needCompleteNode)
	{
		set(NEED_COMPLETE_NODE, needCompleteNode);
	}
	
	/**
	 * 获取是否需要确认前面所有节点
	 */
	public String getIsConfirmAll()
	{
		return getString(IS_CONFIRM_ALL);
	}
	
	/**
	 * 设置是否需要确认前面所有节点
	 */
	public void setIsConfirmAll(String isConfirmAll)
	{
		set(IS_CONFIRM_ALL, isConfirmAll);
	}
	
	/**
	 * 获取创建时间
	 */
	public Date getCreateDate()
	{
		String createDate = getString(CREATE_DATE);
		if (StringUtils.isNotEmpty(createDate))
		{
			return DateHelper.parseString(createDate, DateHelper.pattern_time);
		}
		else
		{
			return null;
		}
	}
	
	/**
	 * 设置创建时间
	 */
	public void setCreateDate(Date createDate)
	{
		set(CREATE_DATE, createDate);
	}
	
	/**
	 * 获取更新时间
	 */
	public Date getUpdateDate()
	{
		String updateDate = getString(UPDATE_DATE);
		if (StringUtils.isNotEmpty(updateDate))
		{
			return DateHelper.parseString(updateDate, DateHelper.pattern_time);
		}
		else
		{
			return null;
		}
	}
	
	/**
	 * 设置更新时间
	 */
	public void setUpdateDate(Date updateDate)
	{
		set(UPDATE_DATE, updateDate);
	}
	
	/**
	 * 获取创建人
	 */
	public Long getCreateUser()
	{
		return getLong(CREATE_USER);
	}
	
	/**
	 * 设置创建人
	 */
	public void setCreateUser(Long createUser)
	{
		set(CREATE_USER, createUser);
	}
	
	/**
	 * 获取最后修改人
	 */
	public Long getLastUpdateUser()
	{
		return getLong(LAST_UPDATEUSER);
	}
	
	/**
	 * 设置最后修改人
	 */
	public void setLastUpdateUser(Long lastUpdateUser)
	{
		set(LAST_UPDATEUSER, lastUpdateUser);
	}
	
	/**
	 * 获取节点对应H5的pageid标识
	 */
	public String getPageId()
	{
		return getString(PAGE_ID);
	}
	
	/**
	 * 设置节点对应H5的pageid标识
	 */
	public void setPageId(Long pageId)
	{
		set(PAGE_ID, pageId);
	}
	
	/**
	 * 获取节点对应资料提交Function功能号
	 */
	public Long getFuncNo()
	{
		return getLong(FUNC_NO);
	}
	
	/**
	 * 设置节点对应资料提交Function功能号
	 */
	public void setFuncNo(Long funcNo)
	{
		set(FUNC_NO, funcNo);
	}
	
	/**
	 * 获取关联节点属性类型
	 */
	public String getNodeType()
	{
		return getString(NODE_TYPE);
	}
	
	/**
	 * 设置关联节点属性类型
	 */
	public void setNodeType(String nodeType)
	{
		set(NODE_TYPE, nodeType);
	}
}
