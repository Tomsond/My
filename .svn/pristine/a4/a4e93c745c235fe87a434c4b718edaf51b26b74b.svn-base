public class OtcOpenServiceImpl implements OtcOpenService
{
	
	private OtcInfoDao otcInfoDao;
	
	private ChoiceAccountRecordService choiceAccountRecordService;
	
	public void setOtcInfoDao(OtcInfoDao otcInfoDao)
	{
		this.otcInfoDao = otcInfoDao;
	}
	
	public void setChoiceAccountRecordService(ChoiceAccountRecordService choiceAccountRecordService)
	{
		this.choiceAccountRecordService = choiceAccountRecordService;
	}
	
	@Override
	public List<ChoiceAccountRecordModel> queryApplyAccountsByBId(Long businessId)
	{
		return choiceAccountRecordService.queryByBusinessId(businessId);
	}
	
	@Override
	public List<OtcInfo> queryAllOtcInfo()
	{
		return otcInfoDao.queryAll();
	}
	
	@Override
	public List<OtcInfo> queryByCodes(String[] codes)
	{
		if (codes == null || codes.length < 0)
		{
			return null;
		}
		return otcInfoDao.queryByCodes(codes);
	}
	
}
