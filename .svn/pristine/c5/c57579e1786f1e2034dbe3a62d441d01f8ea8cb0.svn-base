public class UserEntrustBusinessDaoImpl extends ISMPBaseDao implements UserEntrustBusinessDao
{

    @Override
    public UserEntrustBusinessModel add(UserEntrustBusinessModel userEntrustBusinessModel,Session session)
    {
        userEntrustBusinessModel.setCreateDate(new Date());
        session.insert(EntrustDictionaryInfoContants.T_ISMP_USERENTRUST_BUSINESS, userEntrustBusinessModel);
        return userEntrustBusinessModel;
    }
    
}
