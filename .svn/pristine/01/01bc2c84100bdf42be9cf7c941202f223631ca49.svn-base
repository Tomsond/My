public class VideoNodePropertyConstants
{
	
	/**
	 * @描述:是否启用单向视频
	 * @可选值:0-否，1-是
	 * @节点类型:视频见证
	 */
	public static final String PROPERTY_KEY_STARTUP_DXSP = "startup_dxsp";
	
	/**
	 * @描述:是否启动双向视频
	 * @可选值:0-否，1-是
	 * @节点类型:视频见证
	 */
	public static final String PROPERTY_KEY_STARTUP_SXSP = "startup_sxsp";
	
	/**
	 * @描述:默认视频方式
	 * @可选值:dxsp-单向视频，sxsp-双向视频
	 * @节点类型:视频见证
	 */
	public static final String PROPERTY_KEY_DEFAULT_TYPE = "default_type";
	
	/**
	 * @描述:单向视频服务开始时间
	 * @格式:HH:mm:ss 例如09:11:22
	 * @节点类型:视频见证
	 * @依赖其他节点属性:PROPERTY_KEY_STARTUP_DXSP(是否启用单向视频)
	 */
	public static final String PROPERTY_KEY_DXSP_STARTTIME = "dxsp_starttime";
	
	/**
	 * @描述:单向视频服务结束时间
	 * @格式:HH:mm:ss 例如09:11:22
	 * @节点类型:视频见证
	 * @依赖其他节点属性:PROPERTY_KEY_STARTUP_DXSP(是否启用单向视频)
	 */
	public static final String PROPERTY_KEY_DXSP_ENDTIME = "dxsp_endtime";
	
	/**
	 * @描述:双向视频服务开始时间
	 * @格式:HH:mm:ss 例如09:11:22
	 * @节点类型:视频见证
	 * @依赖其他节点属性:PROPERTY_KEY_STARTUP_SXSP(是否启用双向视频)
	 */
	public static final String PROPERTY_KEY_SXSP_STARTTIME = "sxsp_starttime";
	
	/**
	 * @描述:双向视频服务结束时间
	 * @格式:HH:mm:ss 例如09:11:22
	 * @节点类型:视频见证
	 * @依赖其他节点属性:PROPERTY_KEY_STARTUP_SXSP(是否启用双向视频)
	 */
	public static final String PROPERTY_KEY_SXSP_ENDTIME = "sxsp_endtime";
	
	/**
	 * @描述:单向视频提示语
	 * @格式:字符串
	 * @节点类型:视频见证
	 * @依赖其他节点属性:PROPERTY_KEY_STARTUP_DXSP(是否启用单向视频)
	 */
	public static final String PROPERTY_KEY_DXSP_NOTICE = "dxsp_notice";
	
	/**
	 * @描述:单向视频是否需要审核
	 * @可选值:0-否，1-是
	 * @节点类型:视频见证
	 * @依赖其他节点属性:PROPERTY_KEY_STARTUP_DXSP(是否启用单向视频)
	 */
	public static final String PROPERTY_KEY_DXSP_NEEDCHECK = "dxsp_needcheck";
	
	/**
	 * @描述:双向视频是否需要审核
	 * @可选值:0-否，1-是
	 * @节点类型:视频见证
	 * @依赖其他节点属性:PROPERTY_KEY_STARTUP_SXSP(是否启用双向视频)
	 */
	public static final String PROPERTY_KEY_SXSP_NEEDCHECK = "SXSP_NEEDCHECK";
}
