public class NodePropertyConfigServiceImpl implements NodePropertyConfigService
{
	
	private NodePropertyConfigDao nodePropertyConfigDao;
	
	public void setNodePropertyConfigDao(NodePropertyConfigDao nodePropertyConfigDao)
	{
		this.nodePropertyConfigDao = nodePropertyConfigDao;
	}
	
	@Override
	public List<NodePropertyConfigModel> queryByNodeIdAndState(String nodeId, String state)
	{
		return nodePropertyConfigDao.queryByNodeIdAndState(nodeId, state);
	}

    @Override
    public NodePropertyConfigModel queryByNodeIdAndStateAndPropertyKey(String nodeId, String propertyKey, String state)
    {
        return nodePropertyConfigDao.queryByNodeIdAndStateAndPropertyKey(nodeId, propertyKey, state);
    }
	
}
