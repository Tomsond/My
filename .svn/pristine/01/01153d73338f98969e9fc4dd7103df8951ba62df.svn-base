public class BranchInfoServiceImpl implements BranchInfoService
{

    private BranchExtinfoDao branchExtinfoDao;
    
    public void setBranchExtinfoDao(BranchExtinfoDao branchExtinfoDao){
        this.branchExtinfoDao=branchExtinfoDao;   
    }
    
    @Override
    public BranchExtinfoModel queryByBranchNo(String branchNo)
    {
        return branchExtinfoDao.queryByBranchNo(branchNo);
    }
    
    @Override
	public List<DataRow> queryList(String branch_id) throws Exception {
		return branchExtinfoDao.queryList(branch_id);
	}
}
