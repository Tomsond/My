public class SmsModelModel extends DataRow
{
	
	private static final long serialVersionUID = -6890562575272609910L;
	
	private static final String ID = "id";//ID
	
	private static final String SMS_TYPE = "sms_type";//短信类型
	
	private static final String SMS_MESSAGE = "sms_message";//短信内容
	
	private static final String STATE = "state";//状态
	
	private static final String SMS_LEVEL = "sms_level";//短信级别
	
	private static final String SMS_SEND_TYPE = "sms_send_type";//短信发送类型
	
	
	/**
	 * 获取自增主键
	 */
	public Long getId()
	{
		return getLong(ID);
	}
	
	/**
	 * 设置自增主键
	 */
	public void setId(Long id)
	{
		set(ID, id);
	}
	
	/**
	 * 获取短信类型
	 */
	public String getSmsType()
	{
		return getString(SMS_TYPE);
	}
	
	/**
	 * 设置短信类型
	 */
	public void setSmsType(String smsType)
	{
		set(SMS_TYPE, smsType);
	}
	
	/**
	 * 获取短信内容
	 */
	public String getSmsMessage()
	{
		return getString(SMS_MESSAGE);
	}
	
	/**
	 * 设置短信内容
	 */
	public void setSmsMessage(String smsMessage)
	{
		set(SMS_MESSAGE, smsMessage);
	}
	
	/**
	 * 获取状态
	 */
	public String getState()
	{
		return getString(STATE);
	}
	
	/**
	 * 设置状态
	 */
	public void setState(String state)
	{
		set(STATE, state);
	}
	
	/**
	 * 获取短信级别
	 */
	public String getSmsLevel()
	{
		return getString(SMS_LEVEL);
	}
	
	/**
	 * 设置短信级别
	 */
	public void setSmsLevel(String smsLevel)
	{
		set(SMS_LEVEL, smsLevel);
	}
	
	/**
	 * 获取短信发送类型
	 */
	public String getSmsSendType()
	{
		return getString(SMS_SEND_TYPE);
	}
	
	/**
	 * 设置短信发送类型
	 */
	public void setSmsSendType(String smsSendType)
	{
		set(SMS_SEND_TYPE, smsSendType);
	}
}
