package com.thinkive.fxc.ismp.bus.base.business.service.impl;

import com.thinkive.base.jdbc.DataRow;
import com.thinkive.fxc.ismp.bus.base.business.dao.ProtocolDao;
import com.thinkive.fxc.ismp.bus.base.business.service.ProtocolService;
import com.thinkive.fxc.ismp.bus.business.service.impl.BusinessCommonServiceImpl;

public class ProtocolServiceImpl extends BusinessCommonServiceImpl implements ProtocolService{

	private ProtocolDao protocolDao;
	
	
	public void setProtocolDao(ProtocolDao protocolDao) {
		this.protocolDao = protocolDao;
	}

	public DataRow queryProtocol(String protocol_type) throws Exception {
		return protocolDao.queryProtocol(protocol_type);
	}

}
