package com.thinkive.fxc.ismp.bus.base.basicdata.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 公共-视频见证坐席操作日志表(T_FXC_VIDEO_WITNESS_LOG)模型
 * @版权: Copyright (c) 2010
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 田源
 * @版本: 2.0.0 
 * @创建时间: 2016年8月18日 下午1:42:37
 */
public class VideoWitnessLogModel extends DataRow
{

    private static final long serialVersionUID = 3167649547367766191L;
    
    private static final String ID = "id";

    private static final String CUST_ID = "cust_id";
    
    private static final String SRV_ID   = "srv_id";
    
    private static final String OP_TYPE = "op_type";
    
    private static final String CREATE_DATE = "create_date";
    
    private static final String BIZ_TYPE = "biz_type";
    
    private static final String REJECT_TYPE = "reject_type";
    
    /**
     * 获取自增主键
     */
    public Long getId()
    {
        return getLong(ID);
    }
    
    /**
     * 设置自增主键
     */
    public void setId(Long id)
    {
        set(ID, id);
    }
    
    /**
     * 获取客户ID
     */
    public Long getCustId()
    {
        return getLong(CUST_ID);
    }
    
    /**
     * 设置客户ID
     */
    public void setCustId(Long custId)
    {
        set(CUST_ID, custId);
    }
    
    
    /**
     * 获取坐席ID
     */
    public Long getSrvId()
    {
        return getLong(SRV_ID);
    }
    
    /**
     * 设置坐席ID
     */
    public void setSrvId(Long srvId)
    {
        set(SRV_ID, srvId);
    }
    
    /**
     * 获取操作类型(1:坐席登录;2:坐席退出;3:接入客户;4:视频通过;5:视频驳回;6:修改客户资料;)
     */
    public String getOpType()
    {
        return getString(OP_TYPE);
    }
    
    /**
     * 设置操作类型(1:坐席登录;2:坐席退出;3:接入客户;4:视频通过;5:视频驳回;6:修改客户资料;)
     */
    public void setOpType(String opType)
    {
        set(OP_TYPE, opType);
    }
    
    /**
     * 获取创建时间
     */
    public Date getCreateDate() {
        return (Date) get(CREATE_DATE);
    }

    /**
     * 设置创建时间
     */
    public void setCreateDate(Date createDate) {
        Timestamp timestamp=new Timestamp(createDate.getTime());
        set(CREATE_DATE, timestamp);
    }
    
    /**
     * 获取业务类型
     */
    public String getBizType()
    {
        return getString(BIZ_TYPE);
    }
    
    /**
     * 设置业务类型
     */
    public void setBizType(String bizType)
    {
        set(BIZ_TYPE, bizType);
    }
    
    /**
     * 获取视频驳回类型
     */
    public String getRejectType()
    {
        return getString(REJECT_TYPE);
    }
    
    /**
     * 设置视频驳回类型
     */
    public void setRejectType(String rejectType)
    {
        set(REJECT_TYPE, rejectType);
    }
}
