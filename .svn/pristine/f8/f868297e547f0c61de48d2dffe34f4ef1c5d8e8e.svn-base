package com.thinkive.fxc.ismp.bus.base.business.service.impl;

import java.util.List;

import com.thinkive.fxc.ismp.bus.base.business.dao.LinkmanRecordDao;
import com.thinkive.fxc.ismp.bus.base.business.model.LinkmanRecordModel;
import com.thinkive.fxc.ismp.bus.base.business.service.LinkmanRecordService;

public class LinkmanRecordServiceImpl implements LinkmanRecordService
{
	
	LinkmanRecordDao linkmanRecordDao;
	
	public void setLinkmanRecordDao(LinkmanRecordDao linkmanRecordDao)
	{
		this.linkmanRecordDao = linkmanRecordDao;
	}
	
	@Override
	public LinkmanRecordModel add(LinkmanRecordModel emergencyLinkmanRecordModel)
	{
		return linkmanRecordDao.add(emergencyLinkmanRecordModel);
	}
	
	@Override
	public int update(LinkmanRecordModel emergencyLinkmanRecordModel)
	{
		return linkmanRecordDao.update(emergencyLinkmanRecordModel);
	}
	
	@Override
	public List<LinkmanRecordModel> queryByParams(Long businessId, String nodeId)
	{
		return linkmanRecordDao.queryByParams(businessId, nodeId);
	}
	
}
