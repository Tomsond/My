package com.thinkive.fxc.ismp.bus.base.basicdata.model;

import java.sql.Timestamp;
import java.util.Date;
import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 公共-协议内容表(T_FXC_AGREEMENT_CONTENT)模型
 * @版权: Copyright (c) 2016 
 * @公司: 深圳市思迪信息技术股份有限公司
