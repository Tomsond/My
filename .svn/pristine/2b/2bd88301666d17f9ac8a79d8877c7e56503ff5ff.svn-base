public class AgrementContentModel extends DataRow
{
	
	private static final long serialVersionUID = 5609386759126058506L;
	
	private static final String ID = "id";
	
	private static final String AGREE_NO = "agree_no";
	
	private static final String AGREE_VERSION = "agree_version";
	
	private static final String AGREE_STATUS = "agree_status";
	
	private static final String AGREE_MD5 = "agree_md5";
	
	private static final String AGREE_BRIEF = "agree_brief";
	
	private static final String AGREE_CONTENT = "agree_content";
	
	private static final String CREATE_DATE = "create_date";
	
	private static final String CREATE_BY = "create_by";
	
	private static final String UPDATE_DATE = "update_date";
	
	private static final String UPDATE_BY = "update_by";
	
	/**
	 * 获取自增主键
	 */
	public Long getId()
	{
		return getLong(ID);
	}
	
	/**
	 * 设置自增主键
	 */
	public void setId(Long id)
	{
		set(ID, id);
	}
	
	/**
	 * 获取协议编号
	 */
	public Long getAgreeNo()
	{
		return getLong(AGREE_NO);
	}
	
	/**
	 * 设置协议编号
	 */
	public void setAgreeNo(Long agreeNo)
	{
		set(AGREE_NO, agreeNo);
	}
	
	/**
	 * 获取协议版本
	 */
	public Long getAgreeVersion()
	{
		return getLong(AGREE_VERSION);
	}
	
	/**
	 * 设置协议版本
	 */
	public void setAgreeVersion(Long agreeVersion)
	{
		set(AGREE_VERSION, agreeVersion);
	}
	
	/**
	 * 获取协议状态
	 */
	public String getAgreeStatus()
	{
		return getString(AGREE_STATUS);
	}
	
	/**
	 * 设置协议状态
	 */
	public void setAgreeStatus(String agreeStatus)
	{
		set(AGREE_STATUS, agreeStatus);
	}
	
	/**
	 * 获取协议MD5值
	 */
	public String getAgreeMd5()
	{
		return getString(AGREE_MD5);
	}
	
	/**
	 * 设置协议MD5值
	 */
	public void setAgreeMd5(String agreeMd5)
	{
		set(AGREE_MD5, agreeMd5);
	}
	
	/**
	 * 获取协议简介
	 */
	public String getAgreeBrief()
	{
		return getString(AGREE_BRIEF);
	}
	
	/**
	 * 设置协议简介
	 */
	public void setAgreeBrief(String agreeBrief)
	{
		set(AGREE_BRIEF, agreeBrief);
	}
	
	/**
	 * 获取协议内容
	 */
	public String getAgreeContent()
	{
		return getString(AGREE_CONTENT);
	}
	
	/**
	 * 设置协议内容
	 */
	public void setAgreeContent(String agreeContent)
	{
		set(AGREE_CONTENT, agreeContent);
	}
	
	/**
	 * 获取创建人
	 */
	public Long getCreateBy()
	{
		return getLong(CREATE_BY);
	}
	
	/**
	 * 设置创建人
	 */
	public void setCreateBy(Long createBy)
	{
		set(CREATE_BY, createBy);
	}
	
	/**
	 * 获取修改人
	 */
	public Long getUpdateBy()
	{
		return getLong(UPDATE_BY);
	}
	
	/**
	 * 设置修改人
	 */
	public void setUpdateBy(Long updateBy)
	{
		set(UPDATE_BY, updateBy);
	}
	
	/**
	 * 获取创建时间
	 */
	public Date getCreateDate()
	{
		return (Date) get(CREATE_DATE);
	}
	
	/**
	 * 设置创建时间
	 */
	public void setCreateDate(Date createDate)
	{
		Timestamp timestamp = new Timestamp(createDate.getTime());
		set(CREATE_DATE, timestamp);
	}
	
	/**
	 * 获取更新时间
	 */
	public Date getUpdateDate()
	{
		return (Date) get(UPDATE_DATE);
	}
	
	/**
	 * 设置更新时间
	 */
	public void setUpdateDate(Date updateDate)
	{
		Timestamp timestamp = new Timestamp(updateDate.getTime());
		set(UPDATE_DATE, timestamp);
	}
}
