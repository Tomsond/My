public class SmsSendLogDaoImpl extends ISMPBaseDao implements SmsSendLogDao
{
	
	@Override
	public SmsSendLogModel addSmsSendLog(SmsSendLogModel smsSendLogModel)
	{
		String id = getSeqValue(SystemConstants.DB_ISMP,BusinessTableConstants.T_ISMP_SMS_USERLOG);
		smsSendLogModel.setId(Long.parseLong(id));
		smsSendLogModel.setSendTime(new Date());
		getJdbcTemplate().insert(BusinessTableConstants.T_ISMP_SMS_USERLOG, smsSendLogModel);
		return smsSendLogModel;
	}
}
