public class ThirdBankBusinessInfoVo extends BusinessEntityModel
{

    private static final long serialVersionUID = 859536810067540534L;
    
    private static final String FUND_ACCOUNT="fund_account";
    
    private static final String OP_TYPE="op_type";
    
    private static final String OLD_BANKCODE="old_bankcode";
    
    private static final String OLD_BANKNUM="old_banknum";
    
    private static final String NEW_BANKCODE="new_bankcode";
    
    private static final String NEW_BANKNUM="new_banknum";
    
    private static final String BIND_ERROR_COUNT="bind_error_count";

    private static final String UPDATE_DATE="update_date";

    /**
     *获取资金帐号
     */
    public String getFundAccount()
    {
        return getString(FUND_ACCOUNT);
    }
    
    /**
     *设置资金帐号
     */
    public void setFundAccount(String fundAccount)
    {
        set(FUND_ACCOUNT, fundAccount);
    }
    
    /**
     *获取操作类型 0:注销，1：绑定，2：修改，01：先注销再绑定
     */
    public String getOpType()
    {
        return getString(OP_TYPE);
    }
    
    /**
     *设置操作类型 0:注销，1：绑定，2：修改，01：先注销再绑定
     */
    public void setOpType(String opType)
    {
        set(OP_TYPE, opType);
    }
    
    /**
     *获取注销的银行编号
     */
    public String getOldBankcode()
    {
        return getString(OLD_BANKCODE);
    }
    
    /**
     *设置注销的银行编号
     */
    public void setOldBankcode(String oldBankcode)
    {
        set(OLD_BANKCODE, oldBankcode);
    }
    
    /**
     *获取注销的银行卡号
     */
    public String getOldBanknum()
    {
        return getString(OLD_BANKNUM);
    }
    
    /**
     *设置注销的银行卡号
     */
    public void setOldBanknum(String oldBanknum)
    {
        set(OLD_BANKNUM, oldBanknum);
    }
    
    /**
     *获取绑定的银行编号
     */
    public String getNewBankcode()
    {
        return getString(NEW_BANKCODE);
    }
    
    /**
     *设置绑定的银行编号
     */
    public void setNewBankcode(String newBankcode)
    {
        set(NEW_BANKCODE, newBankcode);
    }
    
    /**
     *获取绑定的银行卡号
     */
    public String getNewBanknum()
    {
        return getString(NEW_BANKNUM);
    }
    
    /**
     *设置绑定的银行卡号
     */
    public void setNewBanknum(String newBanknum)
    {
        set(NEW_BANKNUM, newBanknum);
    }
    
    /**
     *获取绑定错误次数
     */
    public Integer getBindErrorCount()
    {
        return getInt(BIND_ERROR_COUNT);
    }
    
    /**
     *设置绑定错误次数
     */
    public void setBindErrorCount(Integer bindErrorCount)
    {
        set(BIND_ERROR_COUNT, bindErrorCount);
    }

    /**
     * 获取更新时间
     */
    public Date getUpdateDate()
    {
        return (Date) get(UPDATE_DATE);
    }

    /**
     * 设置更新时间
     */
    public void setUpdateDate(Date update_date)
    {
        Timestamp timestamp = new Timestamp(update_date.getTime());
        set(UPDATE_DATE, timestamp);
    }
    
}
