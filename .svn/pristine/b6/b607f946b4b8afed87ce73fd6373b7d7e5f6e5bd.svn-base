package com.thinkive.fxc.ismp.bus.business.assets.thirdbank.vo;

import com.thinkive.fxc.ismp.bus.base.business.model.BusinessEntityModel;

import java.sql.Timestamp;
import java.util.Date;

/**
 * @描述: 三方存管业务实例视图
