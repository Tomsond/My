public class ZkExtendDaoImpl extends ISMPBaseDao implements ZkExtendDao
{
	
	@Override
	public void add(ZkExtendModel zkExtendModel)
	{
		this.getJdbcTemplate().insert(ZkConstants.T_ISMP_B_ZK, zkExtendModel);
	}
	
	@Override
	public int update(ZkExtendModel zkExtendModel)
	{
		Long businessId = zkExtendModel.getBusinessId();
		int count = this.getJdbcTemplate().update(ZkConstants.T_ISMP_B_ZK, zkExtendModel, "business_id", businessId);
		zkExtendModel.setBusinessId(businessId);
		return count;
	}
}
