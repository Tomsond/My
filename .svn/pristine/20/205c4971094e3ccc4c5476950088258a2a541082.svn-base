public class EntrustModel extends DataRow
{

    private static final long serialVersionUID = 3985431685997224755L;
    
    private static final String ID="id";
    
    private static final String ENTRUST_NAME="entrust_name";
    
    private static final String ENTRUST_CODE="entrust_code";
    
    private static final String REMARK="remark";
    
    private static final String ORDER_LINE="order_line";
    
    private static final String CREATE_DATE="create_date";
    
    private static final String IMAGE_URL="image_url";
    
    private static final String AGREE_TYPE="agree_type";
    
    private static final String STATE="state";
    
    private static final String NEED_AGREE="need_agree";
    
    private static final String ALLOW_CLOSE="allow_close";
    
    private static final String ALLOW_OPEN="allow_open";
    
    
    /**
     * 获取主键ID
     */
    public long getId()
    {
        return getLong(ID);
    }
    
    /**
     * 设置主键ID
     */
    public void setId(Long id)
    {
        set(ID, id);
    }
    
    /**
     * 获取委托方式
     */
    public String getEntrustName()
    {
        return getString(ENTRUST_NAME);
    }
    
    /**
     * 设置委托方式
     */
    public void setEntrustName(String entrustName)
    {
        set(ENTRUST_NAME, entrustName);
    }
    
    /**
     * 获取委托方式编号
     */
    public String getEntrustCode()
    {
        return getString(ENTRUST_CODE);
    }
    
    /**
     * 设置委托方式编号
     */
    public void setEntrustCode(String entrustCode)
    {
        set(ENTRUST_CODE, entrustCode);
    }
    
    /**
     * 获取委托方式备注
     */
    public String getRemark()
    {
        return getString(REMARK);
    }
    
    /**
     * 设置委托方式备注
     */
    public void setRemark(String remark)
    {
        set(REMARK, remark);
    }
    
    /**
     * 获取排序值
     */
    public int getOrderLine()
    {
        return getInt(ORDER_LINE);
    }
    
    /**
     * 设置排序值
     */
    public void setOrderLine(Integer orderLine)
    {
        set(ORDER_LINE, orderLine);
    }
    
    /**
     * 获取创建时间
     */
    public Date getCreateDate()
    {
        return (Date) get(CREATE_DATE);
    }
    
    /**
     * 设置创建时间
     */
    public void setCreateDate(Date createDate)
    {
        Timestamp timestamp = new Timestamp(createDate.getTime());
        set(CREATE_DATE, timestamp);
    }
    
    /**
     * 获取图片链接
     */
    public String getImageUrl()
    {
        return getString(IMAGE_URL);
    }
    
    /**
     * 设置图片链接
     */
    public void setImageUrl(String imageUrl)
    {
        set(IMAGE_URL, imageUrl);
    }
    
    /**
     * 获取协议类型
     */
    public String getAgreeType()
    {
        return getString(AGREE_TYPE);
    }
    
    /**
     * 设置协议类型
     */
    public void setAgreeType(String agreeType)
    {
        set(AGREE_TYPE, agreeType);
    }
    
    /**
     * 获取是否需要协议,0:否,1:是
     */
    public String getNeedAgree()
    {
        return getString(NEED_AGREE);
    }
    
    /**
     * 设置是否需要协议,0:否,1:是
     */
    public void setNeedAgree(String needAgree)
    {
        set(NEED_AGREE, needAgree);
    }
    
    /**
     * 获取是否可以关闭,0:否,1:是
     */
    public String getAllowClose()
    {
        return getString(ALLOW_CLOSE);
    }
    
    /**
     * 设置是否可以关闭,0:否,1:是
     */
    public void setAllowClose(String allowClose)
    {
        set(ALLOW_CLOSE, allowClose);
    }
    
    /**
     * 获取是否可以开通,0:否,1:是
     */
    public String getAllowOpen()
    {
        return getString(ALLOW_OPEN);
    }
    
    /**
     * 设置是否可以开通,0:否,1:是
     */
    public void setAllowOpen(String allowOpen)
    {
        set(ALLOW_OPEN, allowOpen);
    }
    
    /**
     * 获取是否固定(不知道哪里用，防止其他项目已在试用，暂时保留)
     */
    public String getState()
    {
        return getString(STATE);
    }
    
    /**
     * 设置是否固定(不知道哪里用，防止其他项目已在试用，暂时保留)
     */
    public void setState(String state)
    {
        set(STATE, state);
    }
    
    //非数据库字段entrust_status
    public String getEntrustStatus()
    {
        return getString("entrust_status");
    }
    
    public void setEntrustStatus(String entrustStatus)
    {
        set("entrust_status", entrustStatus);
    }
    
}
