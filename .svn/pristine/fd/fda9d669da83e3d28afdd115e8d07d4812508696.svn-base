public class NodeLogDaoImpl extends ISMPBaseDao implements NodeLogDao
{
	
	@Override
	public NodeLogModel add(NodeLogModel nodeLogModel)
	{
		String id = getSeqValue(SystemConstants.DB_ISMP,BusinessTableConstants.T_ISMP_NODE_LOG);
		nodeLogModel.setId(Long.valueOf(id));
		nodeLogModel.setCreateDate(new Date());
		this.getJdbcTemplate().insert(BusinessTableConstants.T_ISMP_NODE_LOG, nodeLogModel);
		return nodeLogModel;
	}
	
	@Override
	public NodeLogModel add(NodeLogModel nodeLogModel, Session session)
	{
		String id = getSeqValue(SystemConstants.DB_ISMP,BusinessTableConstants.T_ISMP_NODE_LOG);
		nodeLogModel.setId(Long.valueOf(id));
		nodeLogModel.setCreateDate(new Date());
		session.insert(BusinessTableConstants.T_ISMP_NODE_LOG, nodeLogModel);
		return nodeLogModel;
	}
	
}
