<?xml version="1.0" encoding="GB2312"?>
<Config>
    <Java Desc="JAVA相关配置">
                                                                                                 
        <!--64位JRE目录路径(若不设置或设置为空，则缺省为当前工作目录下的jre目录)-->
        <!--<JREHome></JREHome>-->
                                                                                                 
        <!--JAVA虚拟机参数-->
        <VMOptions>-Xmx1024m -Xms1024m -Xss192k -XX:PermSize=128m -XX:MaxPermSize=256m</VMOptions>
                                                                                                 
        <!--接收启动和停止事件的实现类-->
        <EventClass>com.thinkive.fxc.ismp.bus.base.common.EventSink</EventClass>
                                                                                                 
        <!--需要加载的Class文件路径(多项以"|"分隔)-->
        <ClassPath></ClassPath>
                                                                                                 
        <!--需要加载的库文件目录(多项以"|"分隔)(系统会遍历目录,把目录中jar和zip文件加入搜索路径)-->
        <LibPath></LibPath>
                                                                                                 
        <!--JMX监控端口-->
        <!--<JMXPort>8099</JMXPort>-->
                                                                                                 
        <!--是否显示GC信息(0:不显示 1:显示 缺省为0)-->
        <GCTrace>0</GCTrace>
                                                                                                 
    </Java>
    <Server Desc="服务系统配置">
                                                                                                 
        <!--服务器类型编码(分配的编码)-->
        <ServerType>9902</ServerType>
                                                                                                 
        <!--服务器ID(全局必须唯一)-->
        <ServerID>TBA</ServerID>
                                                                                                 
        <!--服务器名称-->
        <ServerName>思迪信息业务中间件云柜台业务bus</ServerName>
                                                                                                 
        <!--服务器监听端口号-->
        <ListenPort>21106</ListenPort>
                                                                                                 
        <!--服务运行模式(0:同时支持JAVA组件和DLL组件[缺省] 1:只支持JAVA组件  2:只支持DLL组件(此时可以不用带JAVA相关环境))-->
        <ServerMode>0</ServerMode>
                                                                                                 
        <!--监控服务端口号(HTTP协议)-->
        <MonitorPort>21103</MonitorPort>
                                                                                                 
        <!--是否DEBUG模式(0:否[缺省]  1:是)-->
        <IsDebug>0</IsDebug>
                                                                                                 
        <!--允许的最大连接数-->
        <MaxConnNum>10000</MaxConnNum>
                                                                                                 
        <!--子反应器数量(设置为服务器CPU核数)-->
        <SubReactorSize>4</SubReactorSize>
                                                                                                 
        <!--线程池工作线程初始化数量(全局缺省值)
            每个功能配置文件中所有功能号在独立的线程池中运行,此值配置初始线程的缺省值，具体值可以通过每个功能配置文件的属性进行配置)-->
        <WorkThreadNum>10</WorkThreadNum>
                                                                                                 
        <!--线程池最大工作线程数量(全局缺省值,系统压力过大时,服务器会自动增加池中工作线程应对压力，但增加后的总线程数不会超过此值)
            每个功能配置文件中所有功能号在独立的线程池中运行,此值配置最大工作线程的缺省值，具体值可以通过每个功能配置文件的属性进行配置)-->
        <MaxWorkThreadNum>100</MaxWorkThreadNum>
                                                                                                 
        <!--等待队列最大长度(每个工作队列)-->
        <MaxQueueLength>65536</MaxQueueLength>
                                                                                                 
        <!--允许连接本服务器的客户机IP地址列表，以|分隔。若为空，则不限制来源IP(例:SourceIPList=127.0.0.1|10.100.104.218|10.100.104.212)-->
        <SourceIPList></SourceIPList>
                                                                                                 
        <!--客户登陆超时时间(单位:秒,缺省1分钟)-->
        <ClientTimeout>60</ClientTimeout>
                                                                                                 
        <!--日志记录的级别,不区分大小写(ALL<DEBUG<INFO<WARN<ERROR<FATAL<OFF),缺省记录全部-->
        <LogLevel>WARN</LogLevel>
                                                                                                 
        <!--任务执行时间报警值(单位:毫秒)(超过此时间会记录报警日志,缺省为3000)-->
        <TaskTimeWarn>300</TaskTimeWarn>
                                                                                                 
        <!--是否允许详细错误信息(0:否 1:是[缺省])(若允许详细错误信息,则系统会自动在返回的错误信息前增加服务器的相关信息,方便排错)-->
        <EnableDetErrMsg>1</EnableDetErrMsg>
                                                                                                 
        <!--是否需要登录(0:否[缺省] 1:是)-->
        <NeedLogin>0</NeedLogin>
                                                                                                 
        <!--是否对响应数据加密(0:否[缺省]  1:是)-->
        <EncEnable>0</EncEnable>
                                                                                                 
        <!--数据加解密密钥-->
        <Key>216E0D565E294060</Key>
                                                                                                 
        <!--功能模块XML列表(多个文件以"|"分隔)-->
        <ModuleXMLList>function.xml</ModuleXMLList>
                                                                                                 
    </Server>
    <Monitor Desc="监控相关配置">
                                                                                                 
        <!--监控服务端口号(监控客户端连接此端口)-->
        <MonitorPort>20002</MonitorPort>
                                                                                                 
        <!--业务数据格式类型(0:无[缺省] 1:普通业务数据 1:定制业务数据 2:扩展业务数据)-->
        <BusDataType>0</BusDataType>
                                                                                                 
        <!--允许连接本监控服务的客户机IP地址列表，以|分隔。若为空，则不限制来源IP(SourceIPList=127.0.0.1|10.100.104.218|10.100.104.212)-->
        <SourceIPList></SourceIPList>
                                                                                                 
    </Monitor>
</Config>
