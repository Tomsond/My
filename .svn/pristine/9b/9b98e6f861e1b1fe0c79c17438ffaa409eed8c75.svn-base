package com.thinkive.fxc.ismp.bus.base.basicdata.service.impl;

import com.thinkive.fxc.ismp.bus.base.basicdata.dao.AnswerScoreDao;
import com.thinkive.fxc.ismp.bus.base.basicdata.model.AnswerScoreModel;
import com.thinkive.fxc.ismp.bus.base.basicdata.service.AnswerScoreService;


public class AnswerScoreServiceImpl implements AnswerScoreService{
	
	private AnswerScoreDao anserScoreDao;
	
	public void setAnswerScoreDao(AnswerScoreDao answerScoreDao){
		this.anserScoreDao = answerScoreDao;
	}
	
	@Override
	public void addOrUpdateAnswerScore(String business_id, String score, String node_id, String subitemname) {
		// TODO Auto-generated method stub
		AnswerScoreModel answerScoreModel = new AnswerScoreModel();
		answerScoreModel.setScore(score);
		answerScoreModel.setNodeId(node_id);
		answerScoreModel.setSubitemname(subitemname);
		answerScoreModel.setBusinessId(business_id);
		anserScoreDao.addOrUpdateAnswerScore(answerScoreModel);
		
	}

}
