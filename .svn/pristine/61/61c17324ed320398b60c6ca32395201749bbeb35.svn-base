public enum StockEnum
{
	SHAG(ThirdInterfaceConstant.TRDACCT_EXCLS_GRGD, ThirdInterfaceConstant.STKBD_SHA, ThirdTBASConstants.STKBD_TYPE_HAG), //沪市A股账户
	SHBG(ThirdInterfaceConstant.TRDACCT_EXCLS_GRGD, ThirdInterfaceConstant.STKBD_SHB, ThirdTBASConstants.STKBD_TYPE_HBG), //沪市B股账户
	SHFJ(ThirdInterfaceConstant.TRDACCT_EXCLS_GRJJ, ThirdInterfaceConstant.STKBD_SHA, ThirdTBASConstants.STKBD_TYPE_HFJ), //沪市封闭式基金账户
	SHAX(ThirdInterfaceConstant.TRDACCT_EXCLS_GRXY, ThirdInterfaceConstant.STKBD_SHA, ThirdTBASConstants.STKBD_TYPE_HAX), //沪市A股信用证券账户
	SZAG(ThirdInterfaceConstant.TRDACCT_EXCLS_GRGD, ThirdInterfaceConstant.STKBD_SZA, ThirdTBASConstants.STKBD_TYPE_SAG), //深市A股账户
	SZBG(ThirdInterfaceConstant.TRDACCT_EXCLS_GRGD, ThirdInterfaceConstant.STKBD_SZB, ThirdTBASConstants.STKBD_TYPE_SBG), //深市B股账户
	SZFJ(ThirdInterfaceConstant.TRDACCT_EXCLS_GRJJ, ThirdInterfaceConstant.STKBD_SZA, ThirdTBASConstants.STKBD_TYPE_SFJ), //深市封闭式基金账户
	SZAX(ThirdInterfaceConstant.TRDACCT_EXCLS_GRXY, ThirdInterfaceConstant.STKBD_SZA, ThirdTBASConstants.STKBD_TYPE_SAX) //深市A股信用证券账户
	;
	
	private String cls, stkbd, trdacc_type;
	
	public String getCls()
	{
		return cls;
	}
	
	public void setCls(String cls)
	{
		this.cls = cls;
	}
	
	public String getStkbd()
	{
		return stkbd;
	}
	
	public void setStkbd(String stkbd)
	{
		this.stkbd = stkbd;
	}
	
	public String getTrdacc_type()
	{
		return trdacc_type;
	}
	
	public void setTrdacc_type(String trdaccType)
	{
		trdacc_type = trdaccType;
	}
	
	private StockEnum(String cls, String stkbd, String trdaccType)
	{
		this.cls = cls;
		this.stkbd = stkbd;
		this.trdacc_type = trdaccType;
	}
	
	public static StockEnum getEnumByTrdaccType(String type)
	{
		for (StockEnum stock : StockEnum.values())
		{
			if (stock.getTrdacc_type().equals(type))
			{
				return stock;
			}
		}
		return null;
	}
	
	public static StockEnum getEnumByClsAndStkbd(String cls, String stkbd)
	{
		for (StockEnum stock : StockEnum.values())
		{
			if (stock.getCls().equals(cls) && stock.getStkbd().equals(stkbd))
			{
				return stock;
			}
		}
		return null;
	}
}
