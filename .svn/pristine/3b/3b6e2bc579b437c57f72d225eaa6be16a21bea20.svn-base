package com.thinkive.fxc.ismp.bus.base.business.model;

import java.sql.Timestamp;
import java.util.Date;
import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 选择帐号记录表(t_ismp_choice_account_record) 模型
 * @版权: Copyright (c) 2016 
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 田源
 * @版本: 2.0 
 * @创建时间: 2016年8月12日 下午4:42:25
 */
public class ChoiceAccountRecordModel extends DataRow
{
	
	private static final long serialVersionUID = 6931172835529276950L;
	
	private static final String ID = "id";//自增编号
	
	private static final String BUSINESS_ID = "business_id";//业务编号
	
	private static final String NODE_ID = "node_id";//节点标识
	
	private static final String TRDACCOUNT = "trdaccount";//交易帐号
	
	private static final String STKBD = "stkbd";//市场
	
	private static final String STATE = "state";//状态，0：未处理，1：成功，2：失败
	
	private static final String DESCRIPTION = "description";//描述
	
	private static final String CREATE_DATE = "create_date";//创建时间
	
	private static final String UPDATE_DATE = "update_date";//更新时间
	
	/**
	 * 获取自增主键
	 */
	public long getId()
	{
		return getLong(ID);
	}
	
	/**
	 * 设置自增主键
	 */
	public void setId(long id)
	{
		set(ID, id);
	}
	
	/**
	 * 获取业务办理实体ID
	 */
	public Long getBusinessId()
	{
		return getLong(BUSINESS_ID);
	}
	
	/**
	 * 设置业务办理实体ID
	 */
	public void setBusinessId(Long businessId)
	{
		set(BUSINESS_ID, businessId);
	}
	
	/**
	 * 获取节点英文标识
	 */
	public String getNodeId()
	{
		return getString(NODE_ID);
	}
	
	/**
	 * 设置节点英文标识
	 */
	public void setNodeId(String nodeId)
	{
		set(NODE_ID, nodeId);
	}
	
	/**
	 * 获取交易帐号
	 */
	public String getTrdAccount()
	{
		return getString(TRDACCOUNT);
	}
	
	/**
	 * 设置交易帐号
	 */
	public void setTrdAccount(String trdAccount)
	{
		set(TRDACCOUNT, trdAccount);
	}
	
	/**
	 * 获取交易市场
	 */
	public String getStkbd()
	{
		return getString(STKBD);
	}
	
	/**
	 * 设置交易市场
	 */
	public void setStkbd(String stkbd)
	{
		set(STKBD, stkbd);
	}
	
	/**
	 * 获取交易市场
	 */
	public String getState()
	{
		return getString(STATE);
	}
	
	/**
	 * 设置交易市场
	 */
	public void setState(String state)
	{
		set(STATE, state);
	}
	
	/**
	 * 获取开通描述
	 */
	public String getDescription()
	{
		return getString(DESCRIPTION);
	}
	
	/**
	 * 设置开通描述
	 */
	public void setDescription(String description)
	{
		set(DESCRIPTION, description);
	}
	
	/**
	 * 获取创建时间
	 */
	public Date getCreateDate()
	{
		return (Date) get(CREATE_DATE);
	}
	
	/**
	 * 设置创建时间
	 */
	public void setCreateDate(Date createDate)
	{
		Timestamp timestamp = new Timestamp(createDate.getTime());
		set(CREATE_DATE, timestamp);
	}
	
	/**
	 * 获取更新时间
	 */
	public Date getUpdateDate()
	{
		return (Date) get(UPDATE_DATE);
	}
	
	/**
	 * 设置更新时间
	 */
	public void setUpdateDate(Date updateDate)
	{
		Timestamp timestamp = new Timestamp(updateDate.getTime());
		set(UPDATE_DATE, timestamp);
	}
}
