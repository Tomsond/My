package com.thinkive.fxc.ismp.bus.base.business.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;

/**
 * 作者：MIKE
 * 公司：深圳市思迪信息技术股份公司
 * 日期时间：2017年2月24日 下午7:38:02
 * 说明：用户对账单邮箱地址设置实例类
 */
public class UserEmailBILLConfigModel extends DataRow
{
	
	private static final long serialVersionUID = 1282903660362042105L;
	
	private static final String ID = "id"; //自增主键
	
	private static final String USER_ID = "user_id"; //用户id
	
	private static final String STATUS = "status"; //办理状态
	
	private static final String EMAIL = "email"; //邮箱地址
	
	private static final String CREATE_DATE = "create_date"; //创建时间
	
	private static final String UPDATE_DATE = "update_date"; //更新时间
	
	public Long getId()
	{
		return getLong(ID);
	}
	
	public void setId(Long id)
	{
		set(ID, id);
	}
	
	public Long getUserId()
	{
		return getLong(USER_ID);
	}
	
	public void setUserId(int userId)
	{
		set(USER_ID, userId);
	}
	
	public String getStates()
	{
		return getString(STATUS);
	}
	
	public void setStates(String status)
	{
		set(STATUS, status);
	}
	
	public String getemail()
	{
		return getString(EMAIL);
	}
	
	public void setemail(String email)
	{
		set(EMAIL, email);
	}
	
	/**
	 * 获取创建时间
	 */
	public Date getCreateDate()
	{
		return (Date) get(CREATE_DATE);
	}
	
	/**
	 * 设置创建时间
	 */
	public void setCreateDate(Date createDate)
	{
		Timestamp timestamp = new Timestamp(createDate.getTime());
		set(CREATE_DATE, timestamp);
	}
	
	/**
	 * 获取更新时间
	 */
	public Date getUpdateDate()
	{
		return (Date) get(UPDATE_DATE);
	}
	
	/**
	 * 设置更新时间
	 */
	public void setUpdateDate(Date updateDate)
	{
		Timestamp timestamp = new Timestamp(updateDate.getTime());
		set(UPDATE_DATE, timestamp);
	}
}
