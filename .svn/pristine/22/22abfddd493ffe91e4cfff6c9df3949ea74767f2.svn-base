public class FundInfo extends DataRow
{
	
	private static final long serialVersionUID = -144257917754978453L;
	
	private static final String ID = "id"; //基金公司自增id
	
	private static final String NAME = "name"; //基金公司名称
	
	private static final String FIRSTSPELL = "first_spell"; //基金公司拼音首字母
	
	private static final String STATE = "state";//基金公司状态 0：不可开通，1：正常可开通
	
	private static final String AGREETYPE = "agree_type";//协议类型 t_ismp_agreement_type表type_code字段
	
	private static final String TACODE = "tacode";//基金代码
	
	public String getId()
	{
		return getString(ID);
	}
	
	public void setId(String id)
	{
		set(ID, id);
	}
	
	public String getName()
	{
		return getString(NAME);
	}
	
	public void setName(String name)
	{
		set(NAME, name);
	}
	
	public String getFirstSpell()
	{
		return getString(FIRSTSPELL);
	}
	
	public void setFirstSpell(String firstSpell)
	{
		set(FIRSTSPELL, firstSpell);
	}
	
	public String getState()
	{
		return getString(STATE);
	}
	
	public void setState(String state)
	{
		set(STATE, state);
	}
	
	public String getAgreeType()
	{
		return getString(AGREETYPE);
	}
	
	public void setAgreeType(String aggreeType)
	{
		set(AGREETYPE, aggreeType);
	}
	
	public String getTacode()
	{
		return getString(TACODE);
	}
	
	public void setTacode(String tacode)
	{
		set(TACODE, tacode);
	}
}
