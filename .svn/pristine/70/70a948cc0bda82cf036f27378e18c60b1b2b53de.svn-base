public class AgreeNoConfigDaoImpl extends ISMPBaseDao implements AgreeNoConfigDao
{
	
	@Override
	public AgreeNoConfigModel queryByNodeId(String nodeId)
	{
		StringBuilder sql = new StringBuilder();
		sql.append("SELECT ID,NODE_ID,AGREE_NO,SIGN_TYPE,SUBMMIT_THIRD FROM ");
		sql.append(BusinessTableConstants.T_ISMP_AGREE_AGREENO_CONFIG);
		sql.append(" WHERE NODE_ID = ?");
		DataRow queryRsseult = this.getJdbcTemplate().queryMap(sql.toString(), new Object[] { nodeId });
		return ModelUtil.packInfo(queryRsseult, AgreeNoConfigModel.class);
	}
	
}
