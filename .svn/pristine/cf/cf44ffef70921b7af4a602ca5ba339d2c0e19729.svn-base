public class CertUserInfoVo extends DataRow
{
	
	private static final long serialVersionUID = -5788063158454568423L;
	
	private static final String USER_ID = "user_id";
	
	private static final String CLIENT_ID = "certuid";
	
	private static final String IDENTITY_TYPE = "identity_type";
	
	private static final String IDENTITY_NUM = "identity_num";
	
	private static final String NAME = "name";
	
	private static final String NATIONALITY = "nationality";
	
	private static final String CERTUID = "certuid";
	
	/**
	 * 获取用户ID
	 */
	public Long getUserId()
	{
		return getLong(USER_ID);
	}
	
	/**
	 * 设置用户ID
	 */
	public void setUserId(Long userId)
	{
		set(USER_ID, userId);
	}
	
	/**
	 * 获取客户号
	 */
	public Long getClientId()
	{
		return getLong(CLIENT_ID);
	}
	
	/**
	 * 设置客户号
	 */
	public void setClientId(Long clientId)
	{
		set(CLIENT_ID, clientId);
	}
	
	/**
	 * 获取证件类型
	 */
	public String getIdentityType()
	{
		return getString(IDENTITY_TYPE);
	}
	
	/**
	 * 设置证件类型
	 */
	public void setIdentityType(String identityType)
	{
		set(IDENTITY_TYPE, identityType);
	}
	
	/**
	 * 获取证件号码
	 */
	public String getIdentityNum()
	{
		return getString(IDENTITY_NUM);
	}
	
	/**
	 * 设置证件号码
	 */
	public void setIdentityNum(String identityNum)
	{
		set(IDENTITY_NUM, identityNum);
	}
	
	/**
	 * 获取姓名
	 */
	public String getName()
	{
		return getString(NAME);
	}
	
	/**
	 * 设置姓名
	 */
	public void setName(String name)
	{
		set(NAME, name);
	}
	
	/**
	 * 获取证书用户编号
	 */
	public String getCertuid()
	{
		return getString(CERTUID);
	}
	
	/**
	 * 设置证书用户编号
	 */
	public void setCertuid(String name)
	{
		set(NAME, name);
	}
	
	/**
	 * 获取国籍
	 */
	public String getNationality()
	{
		return getString(NATIONALITY);
	}
	
	/**
	 * 设置国籍
	 */
	public void setNationality(String nationality)
	{
		set(NATIONALITY, nationality);
	}
}
