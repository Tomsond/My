 * @创建时间: 2016年9月6日 下午4:02:40
 */
public class UserEntrustBusinessModel extends DataRow
{

    private static final long serialVersionUID = -2148528334394480814L;
    
    private static final String BUSINESS_ID = "business_id"; 
    
    private static final String CHANGE_TYPE="change_type";
    
    private static final String CREATE_DATE="create_date";
    
    private static final String ENTRUST_CODE="entrust_code";
    
    /**
     * 获取业务实例ID
     */
    public Long getBusinessId()
    {
        return getLong(BUSINESS_ID);
    }
    
    /**
     * 设置业务实例ID
     */
    public void setBusinessId(Long businessId)
    {
        set(BUSINESS_ID, businessId);
    }
    
    /**
     * 获取修改类型 0：取消 1：开通 2:批量修改
     */
    public String getChangeType()
    {
        return getString(CHANGE_TYPE);
    }
    
    /**
     * 设置修改类型 0：取消 1：开通 2:批量修改
     */
    public void setChangeType(String changeType)
    {
        set(CHANGE_TYPE, changeType);
    }
    
    /**
     * 获取创建时间
     */
    public Date getCreateDate()
    {
        return (Date) get(CREATE_DATE);
    }
    
    /**
     * 设置创建时间
     */
    public void setCreateDate(Date createDate)
    {
        Timestamp timestamp = new Timestamp(createDate.getTime());
        set(CREATE_DATE, timestamp);
    }
    
    /**
     * 获取委托方式编号
     */
    public String getEntrustCode()
    {
        return getString(ENTRUST_CODE);
    }
    
    /**
     * 设置委托方式编号
     */
    public void setEntrustCode(String entrustCode)
    {
        set(ENTRUST_CODE, entrustCode);
    }
}
