public class BusinessConfigServiceImpl implements BusinessConfigService
{
	
	private BusinessConfigDao businessConfigDao;
	
	public void setBusinessConfigDao(BusinessConfigDao businessConfigDao)
	{
		this.businessConfigDao = businessConfigDao;
	}
	
	@Override
	public BusinessConfigModel queryByBtypeId(Long btypeId)
	{
		return businessConfigDao.queryByBtypeId(btypeId);
	}

	@Override
	public BusinessConfigModel queryByOpSourceAndBusinessCode(String businessCode, String opSource)
	{
		return businessConfigDao.queryByOpSourceAndBusinessCode(businessCode, opSource);
	}
	
}
