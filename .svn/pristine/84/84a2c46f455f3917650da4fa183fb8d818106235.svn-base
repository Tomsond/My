public class BankServiceImpl implements BankService
{
	
	private BankInfoDao bankInfoDao;
	
	public void setBankInfoDao(BankInfoDao bankInfoDao)
	{
		this.bankInfoDao = bankInfoDao;
	}
	
	@Override
	public List<BankInfoModel> queryAll()
	{
		return bankInfoDao.queryAll();
	}
	
	@Override
	public BankInfoModel queryByBankNo(String bankNo)
	{
		return bankInfoDao.queryByBankNo(bankNo);
	}
	
	@Override
	public ThirdBankInfoVo queryVoByLinkBankOrg(String linkBankOrg)
	{
		return bankInfoDao.queryVoByLinkBankOrg(linkBankOrg);
	}
	
	@Override
	public BankInfoModel queryByThirdBankConfigId(Long bankConfigId)
	{
		return bankInfoDao.queryByThirdBankConfigId(bankConfigId);
	}
}
