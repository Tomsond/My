package com.thinkive.fxc.ismp.bus.base.business.model;

import java.sql.Timestamp;
import java.util.Date;
import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 用户资金帐号表(T_ISMP_USER_FUNDACCOUNT)模型
 * @版权: Copyright (c) 2016 
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 田源
 * @版本: 2.0 
 * @创建时间: 2016年8月11日 下午5:32:08
 */
public class UserFundAccountModel extends DataRow
{
	
	private static final long serialVersionUID = -5175020041600068488L;
	
	private static final String ID = "id";
	
	private static final String USER_ID = "user_id";
	
	private static final String FUND_ACCOUNT = "fund_account";
	
	private static final String IS_MAIN = "is_main";
	
	private static final String TYPE = "type";
	
	private static final String PWD = "pwd";
	
	private static final String PWDLOGIN_ERRORTIME = "pwdlogin_errortime";
	
	private static final String STATE = "state";
	
	private static final String CREATE_DATE = "create_date ";
	
	private static final String UPDATE_DATE = "update_date ";
	
	/**
	 * 获取自增主键
	 */
	public Long getId()
	{
		return getLong(ID);
	}
	
	/**
	 * 设置自增主键
	 */
	public void setId(Long id)
	{
		set(ID, id);
	}
	
	/**
	 * 获取用户ID
	 */
	public Long getUserId()
	{
		return getLong(USER_ID);
	}
	
	/**
	 * 设置用户ID
	 */
	public void setUserId(Long userId)
	{
		set(USER_ID, userId);
	}
	
	/**
	 * 获取资金帐号
	 */
	public String getFundAccount()
	{
		return getString(FUND_ACCOUNT);
	}
	
	/**
	 * 设置资金帐号
	 */
	public void setFundAccount(String fundAccount)
	{
		set(FUND_ACCOUNT, fundAccount);
	}
	
	/**
	 * 获取是否为主卡
	 */
	public String getIsMain()
	{
		return getString(IS_MAIN);
	}
	
	/**
	 * 设置是否为主卡
	 */
	public void setIsMain(String isMain)
	{
		set(IS_MAIN, isMain);
	}
	
	/**
	 * 获取资金帐号类型
	 */
	public String getType()
	{
		return getString(TYPE);
	}
	
	/**
	 * 设置资金帐号类型 
	 */
	public void setType(String type)
	{
		set(TYPE, type);
	}
	
	/**
	 * 获取交易密码
	 */
	public String getPwd()
	{
		return getString(PWD);
	}
	
	/**
	 * 设置交易密码
	 */
	public void setPwd(String pwd)
	{
		set(PWD, pwd);
	}
	
	/**
	 * 获取交易密码登录错误次数
	 */
	public int getpwdLoginErrorTime()
	{
		return getInt(PWDLOGIN_ERRORTIME);
	}
	
	/**
	 * 设置交易密码登录错误次数
	 */
	public void setpwdLoginErrorTime(int pwdLoginErrorTime)
	{
		set(PWDLOGIN_ERRORTIME, pwdLoginErrorTime);
	}
	
	/**
	 * 获取资金帐号状态
	 */
	public String getState()
	{
		return getString(STATE);
	}
	
	/**
	 * 设置资金帐号状态
	 */
	public void setState(String state)
	{
		set(STATE, state);
	}
	
	/**
	 * 获取创建时间
	 */
	public Date getCreateDate()
	{
		return (Date) get(CREATE_DATE);
	}
	
	/**
	 * 设置创建时间
	 */
	public void setCreateDate(Date createDate)
	{
		Timestamp timestamp = new Timestamp(createDate.getTime());
		set(CREATE_DATE, timestamp);
	}
	
	/**
	 * 获取更新时间
	 */
	public Date getUpdateDate()
	{
		return (Date) get(UPDATE_DATE);
	}
	
	/**
	 * 设置更新时间
	 */
	public void setUpdateDate(Date updateDate)
	{
		Timestamp timestamp = new Timestamp(updateDate.getTime());
		set(UPDATE_DATE, timestamp);
	}
}
