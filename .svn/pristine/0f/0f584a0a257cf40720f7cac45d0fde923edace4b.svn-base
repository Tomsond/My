package com.thinkive.fxc.ismp.bus.base.basicdata.vo;

import java.util.Date;

import org.apache.commons.lang.StringUtils;

import com.thinkive.base.jdbc.DataRow;
import com.thinkive.base.util.DateHelper;

/**
 * @描述: 问卷视图
 * @版权: Copyright (c) 2010
 * @公司: 深圳市思迪信息技术股份有限公司
