public class ChangeMainFundExtendModel extends DataRow
{
	
	private static final long serialVersionUID = -731544693784816386L;
	
	private static final String BUSINESS_ID = "business_id";
	
	private static final String OLD_ACCOUNT = "old_account";//旧的主资金帐号
	
	private static final String NEW_ACCOUNT = "new_account";//新的主资金帐号
	
	/**
	 *获取业务实例ID
	 */
	public Long getBusinessId()
	{
		return getLong(BUSINESS_ID);
	}
	
	/**
	 *设置业务实例ID
	 */
	public void setBusinessId(Long businessId)
	{
		set(BUSINESS_ID, businessId);
	}
	
	/**
	 *获取旧的主资金帐号
	 */
	public String getOldAccount()
	{
		return getString(OLD_ACCOUNT);
	}
	
	/**
	 *设置旧的主资金帐号
	 */
	public void setOldAccount(String oldAccount)
	{
		set(OLD_ACCOUNT, oldAccount);
	}
	
	/**
	 *获取新的主资金帐号
	 */
	public String getNewAccount()
	{
		return getString(NEW_ACCOUNT);
	}
	
	/**
	 *设置新的主资金帐号
	 */
	public void setNewAccount(String newAccount)
	{
		set(NEW_ACCOUNT, newAccount);
	}
}
