public class CitizenInfoServiceImpl implements CitizenInfoService
{
	
	private CitizenInfoDao citizenInfoDao;
	
	public void setCitizenInfoDao(CitizenInfoDao citizenInfoDao)
	{
		this.citizenInfoDao = citizenInfoDao;
	}
	
	@Override
	public CitizenInfo getCitizenInfoByUserId(String userId)
	{
		return citizenInfoDao.getCitizenInfoByUserId(userId);
	}
	
	@Override
	public int saveCitizenInfo(CitizenInfo citizen)
	{
		return citizenInfoDao.saveCitizenInfo(citizen);
	}
	
	@Override
	public CitizenInfo getCitizenInfoByidNo(String idno)
	{
		return citizenInfoDao.getCitizenInfoByidNo(idno);
	}
	
	@Override
	public int updateCitizenInfo(CitizenInfo citizen)
	{
		return citizenInfoDao.updateCitizenInfo(citizen);
	}
	
}
