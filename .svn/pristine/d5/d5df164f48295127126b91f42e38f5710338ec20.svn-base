package com.thinkive.fxc.ismp.bus.base.basicdata.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 公共-问卷答案表(T_FXC_SURVEY_ANSWER)模型
 * @版权: Copyright (c) 2016 
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 田源
 * @版本: 2.0 
 * @创建时间: 2016年8月16日 下午4:33:34
 */
public class SurveyAnswerModel extends DataRow
{

    private static final long serialVersionUID = -457328755906513412L;
    
    private static final String ID = "id";
    
    private static final String QUE_ID = "que_id";
    
    private static final String NAME = "name";
    
    private static final String ORDER_LINE = "order_line";
    
    private static final String STATE = "state";
    
    private static final String EXT_CODE = "ext_code";
    
    private static final String MARK = "mark";
    
    private static final String IS_TRUE = "is_true";
    
    private static final String CREATE_DATE  = "create_date";
    
    private static final String CREATE_BY = "create_by";
    
    private static final String UPDATE_DATE = "update_date";
    
    private static final String UPDATE_BY = "update_by";
    
    /**
     * 获取自增主键
     */
    public Long getId()
    {
        return getLong(ID);
    }
    
    /**
     * 设置自增主键
     */
    public void setId(Long id)
    {
        set(ID, id);
    }
    
    /**
     * 获取问题ID
     */
    public Long getQueId()
    {
        return getLong(QUE_ID);
    }
    
    /**
     * 设置问题ID
     */
    public void setQueId(Long queId)
    {
        set(QUE_ID, queId);
    }
    
    /**
     * 获取答案内容
     */
    public String getName() {
        return getString(NAME);
    }

    /**
     * 获取答案内容
     */
    public void setName(String name) {
        set(NAME, name);
    }   
    
    /**
     * 获取排序值
     */
    public Long getOrderLine() {
        return getLong(ORDER_LINE);
    }

    /**
     * 获取排序值
     */
    public void setOrderLine(Long orderLine) {
        set(ORDER_LINE, orderLine);
    }
    
    /**
     * 获取状态
     */
    public String getState() {
        return getString(STATE);
    }

    /**
     * 获取状态
     */
    public void setState(String state) {
        set(STATE, state);
    }
    
    /**
     * 获取外部扩展编号
     */
    public String getExtCode() {
        return getString(EXT_CODE);
    }

    /**
     * 获取外部扩展编号
     */
    public void setExtCode(String extCode) {
        set(EXT_CODE, extCode);
    }  
    
    /**
     * 获取分数
     */
    public Long getMark()
    {
        return getLong(MARK);
    }
    
    /**
     * 设置分数
     */
    public void setMark(Long mark)
    {
        set(MARK, mark);
    }
    
    /**
     * 获取是否为正确答案
     */
    public String getIsTrue() {
        return getString(IS_TRUE);
    }

    /**
     * 获取是否为正确答案
     */
    public void setIsTrue(String isTrue) {
        set(IS_TRUE, isTrue);
    }   
    
    /**
     * 获取创建时间
     */
    public Date getCreateDate() {
        return (Date) get(CREATE_DATE);
    }

    /**
     * 设置创建时间
     */
    public void setCreateDate(Date createDate) {
        Timestamp timestamp=new Timestamp(createDate.getTime());
        set(CREATE_DATE, timestamp);
    }
    
    /**
     * 获取更新时间
     */
    public Date getUpdateDate() {
        return (Date) get(UPDATE_DATE);
    }

    /**
     * 设置更新时间
     */
    public void setUpdateDate(Date updateDate) {
        Timestamp timestamp=new Timestamp(updateDate.getTime());
        set(UPDATE_DATE, timestamp);
    }
    
    /**
     * 获取创建人
     */
    public Long getCreateBy() {
        return getLong(CREATE_BY);
    }

    /**
     * 设置创建人
     */
    public void setCreateBy(Long createBy) {
        set(CREATE_BY, createBy);
    }  
    
    /**
     * 获取修改人
     */
    public Long getUpdateBy() {
        return getLong(UPDATE_BY);
    }

    /**
     * 设置修改人
     */
    public void setUpdateBy(Long updateBy) {
        set(UPDATE_BY, updateBy);
    }
    
}
