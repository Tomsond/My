public class AuditTaskInfoServiceImpl implements AuditTaskInfoService
{
	
	AuditTaskInfoDao auditTaskInfoDao;
	
	public void setAuditTaskInfoDao(AuditTaskInfoDao auditTaskInfoDao)
	{
		this.auditTaskInfoDao = auditTaskInfoDao;
	}
	
	@Override
	public AuditTaskInfoModel addTask(AuditTaskInfoModel auditTaskInfo)
	{
		return auditTaskInfoDao.addTask(auditTaskInfo);
	}
	
	@Override
	public AuditTaskInfoModel addTask(Long userId, Long businessId, String businessType)
	{
		AuditTaskInfoModel auditTaskInfo = new AuditTaskInfoModel();
		auditTaskInfo.setUserId(userId);
		auditTaskInfo.setBusinessId(businessId);
		auditTaskInfo.setBusinessType(businessType);
		return auditTaskInfoDao.addTask(auditTaskInfo);
	}
	
}
