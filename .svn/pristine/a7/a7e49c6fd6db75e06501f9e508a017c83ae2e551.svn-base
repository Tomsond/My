package com.thinkive.fxc.ismp.bus.business.dzhtqs.dao.impl;

import java.util.List;

import com.thinkive.base.jdbc.DataRow;
import com.thinkive.fxc.ismp.bus.base.common.ISMPBaseDao;
import com.thinkive.fxc.ismp.bus.business.dzhtqs.dao.SignElectronicContractDao;

/**
 * 电子合同签署业务
 */
public class SignElectronicContractDaoImpl extends ISMPBaseDao implements SignElectronicContractDao
{

    @SuppressWarnings("unchecked")
    @Override
    public List<DataRow> queryFundDealList(String fund_code)
    {
        StringBuilder sql = new StringBuilder();
        sql.append("select * from T_FXC_AGREEMENT_INFO t, T_FXC_AGREEMENT_CONTENT m where t.id=m.agree_no and t.group_id = ?");
        List<DataRow> queryResult = getJdbcTemplate().query(sql.toString(), new Object[] { fund_code});
        return queryResult;
    }
    
}
