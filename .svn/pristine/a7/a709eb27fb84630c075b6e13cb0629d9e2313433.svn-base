	private static final String ID = "id";
	
	private static final String BUSINESS_ID = "business_id";
	
	private static final String NODE_ID = "node_id";
	
	private static final String AGREE_NO = "agree_no";
	
	private static final String AGREE_NAME = "agree_name";
	
	private static final String AGREE_VERSION = "agree_version";
	
	private static final String SIGN_TYPE = "sign_type";
	
	private static final String SIGN_SOURCE_FLAG = "sign_source_flag";
	
	private static final String CERT_DN = "cert_dn";
	
	private static final String PROTOCOL_DCSIGN = "protocol_dcsign";
	
