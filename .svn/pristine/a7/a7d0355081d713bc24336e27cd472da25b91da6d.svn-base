public class ResetPwdExtendModel extends DataRow
{
	
	private static final long serialVersionUID = -7097729614692432744L;
	
	private static final String ID = "id";//业务编号
	
	private static final String BUSINESS_ID = "business_id";//是否完成
	
	private static final String FUNDACCOUNT = "fundaccount";//资金帐号
	
	/**
	 * 获取自增主键
	 */
	public long getId()
	{
		return getLong(ID);
	}
	
	/**
	 * 设置自增主键
	 */
	public void setId(long id)
	{
		set(ID, id);
	}
	
	/**
	 *获取业务办理实例编号 
	 */
	public Long getBusinessId()
	{
		return getLong(BUSINESS_ID);
	}
	
	/**
	 *设置业务办理实例编号 
	 */
	public void SetBusinessId(Long businessId)
	{
		set(BUSINESS_ID, businessId);
	}
	
	/**
	 *获取资金帐号
	 */
	public String getFundAccount()
	{
		return getString(FUNDACCOUNT);
	}
	
	/**
	 *设置资金帐号
	 */
	public void setFundAccount(String fundAccount)
	{
		set(FUNDACCOUNT, fundAccount);
	}
}
