public class AnswerScoreModel extends DataRow{

	/**
	 * 
	 */
	private static final long serialVersionUID = -1893262582480490683L;
	
	private static final String ID = "id";
	
	private static final String BUSINESS_ID = "business_id";
	
	private static final String NODE_ID = "node_id";
	
	private static final String SCORE = "score";
	
	private static final String CREATE_DATE = "create_date";
	
	private static final String UPDATE_DATE = "update_date";
	
	private static final String SUBITEMNAME = "subitemname";
	/**
	 * 业务编号
	 * */
	public void setBusinessId(String business_id){
		set(BUSINESS_ID, business_id);
	}
	
	public String getBusinessId(){
		return getString(BUSINESS_ID);
	}
	
	/**
	 * 主键
	 * **/
	public void setId(String id){
		set(ID, id);
	}
	
	public String getId(){
		return getString(ID);
	}
	
	/**
	 * 节点编号
	 * **/
	public void setNodeId(String node_id){
		set(NODE_ID, node_id);
	}
	
	public String getNodeId(){
		return getString(NODE_ID);
	}
	
	/**
	 * 分数
	 * **/
	public void setScore(String score){
		set(SCORE, score);
	}
	
	public String getScore(){
		return getString(SCORE);
	}
	
	/**
	 * 获取创建时间
	 */
	public Date getCreateDate()
	{
		return (Date) get(CREATE_DATE);
	}
	
	/**
	 * 设置创建时间
	 */
	public void setCreateDate(Date createDate)
	{
		Timestamp timestamp = new Timestamp(createDate.getTime());
		set(CREATE_DATE, timestamp);
	}
	
	/**
	 * 获取更新时间
	 */
	public Date getUpdateDate()
	{
		return (Date) get(UPDATE_DATE);
	}
	
	/**
	 * 设置更新时间
	 */
	public void setUpdateDate(Date updateDate)
	{
		Timestamp timestamp = new Timestamp(updateDate.getTime());
		set(UPDATE_DATE, timestamp);
	}
	
	public void setSubitemname(String subitemname){
		set(SUBITEMNAME, subitemname);
	}
	
	public String getSubitemname(String subitemname){
		return getString(SUBITEMNAME);
	}
}
