package com.thinkive.fxc.ismp.bus.base.business.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 节点属性配置表<p>T_ISMP_NODE_PROPERTY_CONFIG</p>
 * @版权: Copyright (c) 2016 
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 刘恺
 * @版本: 2.0 
 * @创建时间: 2016年8月12日 下午3:28:29
 */
public class NodePropertyConfigModel extends DataRow
{
	
	private static final long serialVersionUID = 8217543528485322364L;
	
	private static final String ID = "id";//自增编号
	
	private static final String NODE_ID = "node_id";//节点英文标识
	
	private static final String PROPERTY_KEY = "property_key";//属性标识，例如"STARTUP_DXSP",属性标识对应数据字典用常量来定义
	
	private static final String PROPERTY_VALUE = "property_value";//属性值
	
	private static final String DESCRITION = "descrition";//配置描述
	
	private static final String CREATE_DATE = "create_date";//创建时间
	
	private static final String UPDATE_DATE = "update_date";//最后更新时间
	
	private static final String CREATE_USER = "create_user";//创建人
	
	private static final String LAST_UPDATEUSER = "last_updateuser";//最后修改人
	
	private static final String STATE = "state";//状态 0:无效，1：有效
	
	/**
	 * 获取自增主键
	 */
	public Long getId()
	{
		return getLong(ID);
	}
	
	/**
	 * 设置自增主键
	 */
	public void setId(Long id)
	{
		set(ID, id);
	}
	
	/**
	 * 获取业务类型编号
	 */
	public String getNodeId()
	{
		return getString(NODE_ID);
	}
	
	/**
	 * 设置业务类型编号
	 */
	public void setNodeId(String nodeId)
	{
		set(NODE_ID, nodeId);
	}
	
	/**
	 * 获取属性标识
	 */
	public String getPropertyKey()
	{
		return getString(PROPERTY_KEY);
	}
	
	/**
	 * 设置属性标识
	 */
	public void setPropertyKey(String propertyKey)
	{
		set(PROPERTY_KEY, propertyKey);
	}
	
	/**
	 * 获取属性值
	 */
	public String getPropertyValue()
	{
		return getString(PROPERTY_VALUE);
	}
	
	/**
	 * 设置属性值
	 */
	public void setPropertyValue(String propertyValue)
	{
		set(PROPERTY_VALUE, propertyValue);
	}
	
	/**
	 * 获取属性值
	 */
	public String getDescrition()
	{
		return getString(DESCRITION);
	}
	
	/**
	 * 设置属性值
	 */
	public void setDescrition(String descrition)
	{
		set(DESCRITION, descrition);
	}
	
	/**
	 * 获取创建人
	 */
	public String getCreateUser()
	{
		return getString(CREATE_USER);
	}
	
	/**
	 * 设置创建人
	 */
	public void setCreateUser(String createUser)
	{
		set(CREATE_USER, createUser);
	}
	
	/**
	 * 获取最后修改人
	 */
	public String getLastUpdateUser()
	{
		return getString(LAST_UPDATEUSER);
	}
	
	/**
	 * 设置最后修改人
	 */
	public void setLastUpdateUser(String lastUpdateUser)
	{
		set(LAST_UPDATEUSER, lastUpdateUser);
	}
	
	/**
	 * 获取创建时间
	 */
	public Date getCreateDate()
	{
		return (Date) get(CREATE_DATE);
	}
	
	/**
	 * 设置创建时间
	 */
	public void setCreateDate(Date createDate)
	{
	    Timestamp timestamp=new Timestamp(createDate.getTime());
		set(CREATE_DATE, timestamp);
	}
	
	/**
	 * 获取更新时间
	 */
	public Date getUpdateDate()
	{
		return (Date) get(UPDATE_DATE);
	}
	
	/**
	 * 设置更新时间
	 */
	public void setUpdateDate(Date updateDate)
	{
	    Timestamp timestamp=new Timestamp(updateDate.getTime());
		set(UPDATE_DATE, timestamp);
	}
	
	/**
	 * 获取状态
	 */
	public String getState()
	{
		return getString(STATE);
	}
	
	/**
	 * 设置状态
	 */
	public void setState(String state)
	{
		set(STATE, state);
	}
	
}
