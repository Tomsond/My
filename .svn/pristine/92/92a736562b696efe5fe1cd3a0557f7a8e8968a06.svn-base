package com.thinkive.fxc.ismp.bus.base.business.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 影像资料上传类型配置表(t_ismp_image_imagetype_config)模型
 * @版权: Copyright (c) 2016 
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 田源
 * @版本: 2.0 
 * @创建时间: 2016年8月12日 下午4:10:24
 */
public class ImageTypeConfigModel extends DataRow
{

    private static final long serialVersionUID = 6763909637955632736L;
    
    private static final String ID  = "id";
    
    private static final String NODE_ID = "node_id";
    
    private static final String TYPE_ID = "type_id";
    
    private static final String RECOGNI_TYPE = "recogni_type";
    
    private static final String NEED_OCR = "need_ocr";
    
    private static final String CREATE_DATE = "create_date";
    
    private static final String UPDATE_DATE = "update_date";
    
    /**
     * 获取自增主键
     */
    public Long getId() {
        return getLong(ID);
    }

    /**
     * 设置自增主键
     */
    public void setId(Long id) {
        set(ID, id);
    }
    
    /**
     * 获取节点英文标识
     */
    public String getNodeId() {
        return getString(NODE_ID);
    }

    /**
     * 设置节点英文标识
     */
    public void setNodeId(String nodeId) {
        set(NODE_ID, nodeId);
    }
    
    /**
     * 获取影像类型
     */
    public String getTypeId() {
        return getString(TYPE_ID);
    }

    /**
     * 设置影像类型
     */
    public void setTypeId(String typeId) {
        set(TYPE_ID, typeId);
    }
    
    /**
     * 获取是否识别
     */
    public String getRecogniType() {
        return getString(RECOGNI_TYPE);
    }

    /**
     * 设置是否识别
     */
    public void setRecogniType(String recogniType) {
        set(RECOGNI_TYPE, recogniType);
    }
    
    /**
     * 获取是否需要OCR识别
     */
    public String getNeedOcr() {
        return getString(NEED_OCR);
    }

    /**
     * 设置是否需要OCR识别
     */
    public void setNeedOcr(String needOcr) {
        set(NEED_OCR, needOcr);
    }
    
    /**
     * 获取创建时间
     */
    public Date getCreateDate() {
       return (Date) get(CREATE_DATE) ;
    }

    /**
     * 设置创建时间
     */
    public void setCreateDate(Date createDate) {
        Timestamp timestamp=new Timestamp(createDate.getTime());
        set(CREATE_DATE, timestamp);
    }
    
    /**
     * 获取更新时间
     */
    public Date getUpdateDate() {
       return (Date) get(UPDATE_DATE);
    }

    /**
     * 设置更新时间
     */
    public void setUpdateDate(Date updateDate) {
        Timestamp timestamp=new Timestamp(updateDate.getTime());
        set(UPDATE_DATE, timestamp);
    }
}
