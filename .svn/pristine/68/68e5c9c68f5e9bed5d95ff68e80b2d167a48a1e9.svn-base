package com.thinkive.fxc.ismp.bus.base.business.model;

import java.sql.Timestamp;
import java.util.Date;
import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 影像资料记录表<p>t_ismp_image_record</p>
 * @版权: Copyright (c) 2016 
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 尹杰民
 * @版本: 2.0 
 * @创建时间: 2016年8月12日 下午3:28:29
 */
public class ImageRecordModel extends DataRow
{
	
	private static final long serialVersionUID = -4330515641853503339L;
	
	private static final String ID = "id";
	
	private static final String BUSINESS_ID = "business_id";
	
	private static final String NODE_ID = "node_id";
	
	private static final String SIVE_TYPE = "sive_type";
	
	private static final String MEDIA_CODE = "media_code";
	
	private static final String IMAGE_PATH = "image_path";
	
	private static final String IMAGE_DATA = "image_data";
	
	private static final String SECRET_KEY = "secret_key";
	
	private static final String CREATE_DATE = "create_date";
	
	private static final String UPDATE_DATE = "update_date";
	
	/**
	 * 获取自增主键
	 */
	public Long getId()
	{
		return getLong(ID);
	}
	
	/**
	 * 设置自增主键
	 */
	public void setId(Long id)
	{
		set(ID, id);
	}
	
	/**
	 * 获取业务流水ID
	 */
	public Long getBusinessId()
	{
		return getLong(BUSINESS_ID);
	}
	
	/**
	 * 设置业务流水ID
	 */
	public void setBusinessId(Long businessId)
	{
		set(BUSINESS_ID, businessId);
	}
	
	/**
	 * 获取业务类型编号
	 */
	public String getNodeId()
	{
		return getString(NODE_ID);
	}
	
	/**
	 * 设置业务类型编号
	 */
	public void setNodeId(String nodeId)
	{
		set(NODE_ID, nodeId);
	}
	
	/**
	 * 获取文件保存位置
	 */
	public String getSiveType()
	{
		return getString(SIVE_TYPE);
	}
	
	/**
	 * 设置文件保存位置
	 */
	public void setSiveType(String siveType)
	{
		set(SIVE_TYPE, siveType);
	}
	
	/**
	 * 获取影像类别
	 */
	public String getMediaCode()
	{
		return getString(MEDIA_CODE);
	}
	
	/**
	 * 设置影像类别
	 */
	public void setMediaCode(String mediaCode)
	{
		set(MEDIA_CODE, mediaCode);
	}
	
	/**
	 * 获取存储路径
	 */
	public String getImagePath()
	{
		return getString(IMAGE_PATH);
	}
	
	/**
	 * 设置影像类别
	 */
	public void setImagePath(String imagePath)
	{
		set(IMAGE_PATH, imagePath);
	}
	
	/**
	 * 获取图片数据
	 */
	public String getImageData()
	{
		return getString(IMAGE_DATA);
	}
	
	/**
	 * 设置图片数据
	 */
	public void setImageData(String imageData)
	{
		set(IMAGE_DATA, imageData);
	}
	
	/**
	 * 获取文件密钥
	 */
	public String getSecretKey()
	{
		return getString(SECRET_KEY);
	}
	
	/**
	 * 设置文件密钥
	 */
	public void setSecretKey(String secretKey)
	{
		set(SECRET_KEY, secretKey);
	}
	
	/**
	 * 获取创建时间
	 */
	public Date getCreateDate()
	{
		return (Date) get(CREATE_DATE);
	}
	
	/**
	 * 设置创建时间
	 */
	public void setCreateDate(Date createDate)
	{
		Timestamp timestamp = new Timestamp(createDate.getTime());
		set(CREATE_DATE, timestamp);
	}
	
	/**
	 * 获取更新时间
	 */
	public Date getUpdateDate()
	{
		return (Date) get(UPDATE_DATE);
	}
	
	/**
	 * 设置更新时间
	 */
	public void setUpdateDate(Date updateDate)
	{
		Timestamp timestamp = new Timestamp(updateDate.getTime());
		set(UPDATE_DATE, timestamp);
	}
	
}
