package com.thinkive.fxc.ismp.bus.base.basicdata.dao.impl;

import java.util.Date;

import com.thinkive.fxc.ismp.bus.base.basicdata.contants.BaseTableConstants;
import com.thinkive.fxc.ismp.bus.base.basicdata.dao.ProtocolPipeliningDao;
import com.thinkive.fxc.ismp.bus.base.basicdata.model.ProtocolPipeliningModel;
import com.thinkive.fxc.ismp.bus.base.common.ISMPBaseDao;

public class ProtocolPipeliningDaoImpl extends ISMPBaseDao implements ProtocolPipeliningDao{

	@Override
	public int protocolPipeliningInsert(ProtocolPipeliningModel modeel)  throws Exception{
		    String branchId = getSeqValue(BaseTableConstants.T_ISMP_PROTOCOLPIPELINING);
		    modeel.setId(Long.valueOf(branchId));
		    modeel.setCreateDate(new Date());
		    modeel.setUpdateDate(new Date());
	        int count = this.getJdbcTemplate().insert(BaseTableConstants.T_ISMP_PROTOCOLPIPELINING, modeel);
	        return count;
	}
}