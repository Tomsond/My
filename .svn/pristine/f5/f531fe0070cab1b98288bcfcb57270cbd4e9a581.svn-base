 * @版本: 2.0 
 * @创建时间: 2016年8月12日 上午11:07:30
 */
public class ProcNodeDefineModel extends DataRow
{

    private static final long serialVersionUID = 7798836514254505198L;
    
    private static final String NODE_ID = "node_id";
    
    private static final String BUSINESS_CODE = "business_code";
    
    private static final String NODE_NAME = "node_name";
    
    private static final String PAGE_ID = "page_id";
    
    private static final String FUNC_NO = "func_no";
    
    private static final String NODE_TYPE = "node_type";
    
    private static final String CONFIG_ID = "config_id";
    
    private static final String CREATE_DATE = "create_date";
    
    private static final String UPDATE_DATE = "update_date";
    
    private static final String CREATE_USER = "create_user";
    
    private static final String LAST_UPDATEUSER = "last_updateuser";
    
    /**
     * 获取节点英文标识
     */
    public String getNodeId() {
        return getString(NODE_ID);
    }

    /**
     * 设置节点英文标识
     */
    public void setNodeId(String nodeId) {
        set(NODE_ID, nodeId);
    }
    
    /**
     * 获取业务类型编号
     */
    public String getBusinessCode() {
        return getString(BUSINESS_CODE);
    }

    /**
     * 设置业务类型编号
     */
    public void setBusinessCode(String businessCode) {
        set(BUSINESS_CODE, businessCode);
    } 
    
    /**
     * 获取节点名称
     */
    public String getNodeName() {
        return getString(NODE_NAME);
    }

    /**
     * 设置节点名称
     */
    public void setNodeName(String nodeName) {
        set(NODE_NAME, nodeName);
    }   
    
    /**
     * 获取节点对应H5的pageid标识
     */
    public String getPageId() {
        return getString(PAGE_ID);
    }

    /**
     * 设置节点对应H5的pageid标识
     */
    public void setPageId(String pageId) {
        set(PAGE_ID, pageId);
    }   
    
    /**
     * 获取节点对应资料提交Function功能号
     */
    public Long getFuncNo() {
        return getLong(FUNC_NO);
    }

    /**
     * 设置节点对应资料提交Function功能号
     */
    public void setFuncNo(Long funcNo) {
        set(FUNC_NO, funcNo);
    }   
    
    
    /**
     * 获取关联节点属性类型
     */
    public String getNodeType() {
        return getString(NODE_TYPE);
    }

    /**
     * 设置关联节点属性类型
     */
    public void setNodeType(String nodeType) {
        set(NODE_TYPE, nodeType);
    } 
    
    /**
     * 获取节点属性配置ID
     */
    public String getConfigId() {
        return getString(CONFIG_ID);
    }

    /**
     * 设置节点属性配置ID
     */
    public void setConfigId(String configId) {
        set(CONFIG_ID, configId);
    }
    
    /**
     * 获取创建时间
     */
    public Date getCreateDate() {
        return (Date) get(CREATE_DATE);
    }

    /**
     * 设置创建时间
     */
    public void setCreateDate(Date createDate) {
        Timestamp timestamp=new Timestamp(createDate.getTime());
        set(CREATE_DATE, timestamp);
    }
    
    /**
     * 获取更新时间
     */
    public Date getUpdateDate() {
       return (Date) get(UPDATE_DATE);
    }

    /**
     * 设置更新时间
     */
    public void setUpdateDate(Date updateDate) {
        Timestamp timestamp=new Timestamp(updateDate.getTime());
        set(UPDATE_DATE, timestamp);
    }
    
    /**
     * 获取创建人
     */
    public Long getCreateUser() {
        return getLong(CREATE_USER);
    }

    /**
     * 设置创建人
     */
    public void setCreateUser(Long createUser) {
        set(CREATE_USER, createUser);
    }  
    
    /**
     * 获取最后修改人
     */
    public Long getLastUpdateUser() {
        return getLong(LAST_UPDATEUSER);
    }

    /**
     * 设置最后修改人
     */
    public void setLastUpdateUser(Long lastUpdateUser) {
        set(LAST_UPDATEUSER, lastUpdateUser);
    }  
}
