package com.thinkive.fxc.ismp.bus.base.business.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 业务流程节点定义表(T_ISMP_PROCNODE_DEFINE)模型
 * @版权: Copyright (c) 2016 
 * @公司: 深圳市思迪信息技术股份有限公司
