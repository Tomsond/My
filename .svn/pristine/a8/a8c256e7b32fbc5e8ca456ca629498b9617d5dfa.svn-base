package com.thinkive.fxc.ismp.bus.business.smjj.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 私募基金资产记录表<p>t_ismp_sm_assets</p>
 * @版权: Copyright (c) 2016 
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 尹杰民
 * @版本: 2.0 
 * @创建时间: 2016年8月12日 下午3:28:29
 */
public class SmjjAssetsRecordModel extends DataRow
{
    
    private static final long   serialVersionUID = -4330515641853503339L;
    
    private static final String ID               = "id";
    
    private static final String BUSINESS_ID      = "business_id";
    
    private static final String USER_ID          = "user_id";
    
    private static final String REAL_ASSETS      = "real_assets";
    
    private static String FIRST_TRADE_TIME      = "first_trade_time";//用户真正的首次交易日，柜台查询出的首次交易日
    
    private static String REAL_ASSETS_FLAG      = "real_assets_flag";//内部资产是否满足 0：不满足 1：满足
    
    private static String TRADE_TIME_FLAG      = "trade_time_flag";//交易经验是否满足 0：不满足 1：满足
    
    private static String AUTHEN_TYPE      = "authen_type";//认证类型：1：收入证明 2：家庭金融资产证明
    
    private static String RISK_NAME      = "risk_name";//风险等级名称
    
    private static final String CREATE_DATE      = "create_date";
    
    private static final String UPDATE_DATE      = "update_date";
    
    /**
     * 获取自增主键
     */
    public Long getId()
    {
        return getLong(ID);
    }
    
    /**
     * 设置自增主键
     */
    public void setId(Long id)
    {
        set(ID, id);
    }
    
    /**
     * 获取业务流水ID
     */
    public Long getBusinessId()
    {
        return getLong(BUSINESS_ID);
    }
    
    /**
     * 设置业务流水ID
     */
    public void setBusinessId(Long businessId)
    {
        set(BUSINESS_ID, businessId);
    }
    
    /**
     * 获取用户ID
     */
    public Long getUserId()
    {
        return getLong(USER_ID);
    }
    
    /**
     * 设置用户ID
     */
    public void setUserId(Long userId)
    {
        set(USER_ID, userId);
    }
    
    /**
     * 获取创建时间
     */
    public Date getCreateDate()
    {
        return (Date) get(CREATE_DATE);
    }
    
    /**
     * 设置创建时间
     */
    public void setCreateDate(Date createDate)
    {
        Timestamp timestamp = new Timestamp(createDate.getTime());
        set(CREATE_DATE, timestamp);
    }
    
    /**
     * 获取更新时间
     */
    public Date getUpdateDate()
    {
        return (Date) get(UPDATE_DATE);
    }
    
    /**
     * 设置更新时间
     */
    public void setUpdateDate(Date updateDate)
    {
        Timestamp timestamp = new Timestamp(updateDate.getTime());
        set(UPDATE_DATE, timestamp);
    }
    
    /**
     * @return the assets
     */
    public String getAssets()
    {
        return getString(REAL_ASSETS);
    }
    
    /**
     * @return the assets
     */
    public void setAssets(String real_assets)
    {
        set(REAL_ASSETS, real_assets);
    }

    public String getFirstTradeTime()
    {
        return getString(FIRST_TRADE_TIME);
    }

    public void setFirstTradeTime(String firstTradeTime)
    {
        set(FIRST_TRADE_TIME, firstTradeTime);
    }

    public String getRealAssetsFlag()
    {
        return getString(REAL_ASSETS_FLAG);
    }

    public void setRealAssetsFlag(String realAssetsFlag)
    {
        set(REAL_ASSETS_FLAG, realAssetsFlag);
    }

    public String getTradeTimeFlag()
    {
        return getString(TRADE_TIME_FLAG);
    }

    public void setTradeTimeFlag(String tradeTimeFlag)
    {
        set(TRADE_TIME_FLAG, tradeTimeFlag);
    }

    public String getAuthenType()
    {
        return getString(AUTHEN_TYPE);
    }

    public void setAuthenType(String authenType)
    {
        set(AUTHEN_TYPE, authenType);
    }

    public String getRiskName()
    {
        return getString(RISK_NAME);
    }

    public void setRiskName(String riskName)
    {
        set(RISK_NAME, riskName);
    }
    
    
}
