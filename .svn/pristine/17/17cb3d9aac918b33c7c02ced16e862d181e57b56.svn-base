public class MemcachedServiceImpl implements CacheService
{
	
	@Override
	public boolean setCache(String key, String value, int save_time)
	{
		MemcachedGroupClient memcachedGroupClient = new MemcachedGroupClient();
		boolean flag = memcachedGroupClient.set(key, value, save_time);
		return flag;
	}
	
	@Override
	public String getCache(String key)
	{
		MemcachedGroupClient memcachedGroupClient = new MemcachedGroupClient();
		String value = (String) memcachedGroupClient.get(key);
		return value;
	}
}
