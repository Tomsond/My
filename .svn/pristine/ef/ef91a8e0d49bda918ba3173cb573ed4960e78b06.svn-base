public class IdNoInfoModel extends DataRow
{

    private static final long serialVersionUID = 3557838737877940220L;
    
    private static final String ID = "id";
    
    private static final String BIRTH_DATE = "birth_date";
    
    private static final String CITIZEN_ID_NUMBER = "citizen_id_number";
    
    private static final String NAME = "name";
    
    private static final String PHOTOS = "photos";
    
    private static final String SEXCODE = "sexcode";
    
    private static final String PHOTO_PATH = "photo_path";
    
    private static final String CREATE_DATE = "create_date";
    
    private static final String UPDATE_DATE = "update_date";
    
    private static final String SOURCE = "source";
    
    private static final String SECRET_KEY = "secret_key";
    
    /**
     * 获取自增主键
     */
    public Long getId()
    {
        return getLong(ID);
    }
    
    /**
     * 设置自增主键
     */
    public void setId(Long id)
    {
        set(ID, id);
    }
    
    /**
     * 获取出生日期
     */
    public String getBirthDate()
    {
        return getString(BIRTH_DATE);
    }
    
    /**
     * 设置出生日期
     */
    public void setBirthDate(String birthDate)
    {
        set(BIRTH_DATE, birthDate);
    }
    
    /**
     * 获取身份证号
     */
    public String getCitizenIdNumber()
    {
        return getString(CITIZEN_ID_NUMBER);
    }
    
    /**
     * 设置身份证号
     */
    public void setCitizenIdNumber(String citizenIdNumber)
    {
        set(CITIZEN_ID_NUMBER, citizenIdNumber);
    }
    
    /**
     * 获取姓名
     */
    public String getName()
    {
        return getString(NAME);
    }
    
    /**
     * 设置姓名
     */
    public void setName(String name)
    {
        set(NAME, name);
    }
    
    /**
     * 获取照片
     */
    public String getPhotos()
    {
        return getString(PHOTOS);
    }
    
    /**
     * 设置照片
     */
    public void setPhotos(String photos)
    {
        set(PHOTOS, photos);
    }
    
    /**
     * 获取性别
     */
    public String getSexCode()
    {
        return getString(SEXCODE);
    }
    
    /**
     * 设置性别
     */
    public void setSexCode(String sexCode)
    {
        set(SEXCODE, sexCode);
    }
    
    /**
     * 获取图片在文件服务器存储路径
     */
    public String getPhotoPath()
    {
        return getString(PHOTO_PATH);
    }
    
    /**
     * 设置图片在文件服务器存储路径
     */
    public void setPhotoPath(String photoPath)
    {
        set(PHOTO_PATH, photoPath);
    }
    
    /**
     * 获取创建时间
     */
    public Date getCreateDate() {
        return (Date) get(CREATE_DATE);
    }

    /**
     * 设置创建时间
     */
    public void setCreateDate(Date createDate) {
        Timestamp timestamp=new Timestamp(createDate.getTime());
        set(CREATE_DATE, timestamp);
    }
    
    /**
     * 获取更新时间
     */
    public Date getUpdateDate() {
        return  (Date) get(UPDATE_DATE);
    }

    /**
     * 设置更新时间
     */
    public void setUpdateDate(Date updateDate) {
        Timestamp timestamp=new Timestamp(updateDate.getTime());
        set(UPDATE_DATE, timestamp);
    }
    
    /**
     * 获取公安校验渠道类型
     */
    public String getSource()
    {
        return getString(SOURCE);
    }
    
    /**
     * 设置公安校验渠道类型
     */
    public void setSource(String source)
    {
        set(SOURCE, source);
    }
    
    /**
     * 获取文件密钥
     */
    public String getSecretKey()
    {
        return getString(SECRET_KEY);
    }
    
    /**
     * 设置文件密钥
     */
    public void setSecretKey(String secretKey)
    {
        set(SECRET_KEY, secretKey);
    }
}
