package com.thinkive.fxc.ismp.bus.base.basicdata.model;

import java.sql.Timestamp;
import java.util.Date;

import com.thinkive.base.jdbc.DataRow;

/**
 * @描述: 公共-营业部扩展信息表(T_FXC_BRANCH_EXTINFO) 模型
 * @版权: Copyright (c) 2016 
 * @公司: 深圳市思迪信息技术股份有限公司
 * @作者: 尹杰民
 * @版本: 2.0 
 * @创建时间: 2016年8月17日 下午1:57:42
 */
public class BranchExtinfoModel extends DataRow
{
    
    private static final long   serialVersionUID = 5324883438905282039L;
    
    private static final String BRANCH_ID        = "branch_id";
    
    private static final String CUST_CODE        = "cust_code";
    
    private static final String CUST_PWD         = "cust_pwd";
    
    private static final String JGDM             = "jgdm";
    
    private static final String WDDM             = "wddm";
    
    private static final String CREATE_DATE      = "create_date";
    
    private static final String UPDATE_DATE      = "update_date";
    
    /**
     * 获取营业部ID
     */
    public Long getBranchId()
    {
        return getLong(BRANCH_ID);
    }
    
    /**
     * 设置营业部ID
     */
    public void setBranchId(Long branchId)
    {
        set(BRANCH_ID, branchId);
    }
    
    /**
     * 获取柜员编号
     */
    public String getCustCode()
    {
        return getString(CUST_CODE);
    }
    
    /**
     * 设置柜员编号
     */
    public void setCustCode(String custCode)
    {
        set(CUST_CODE, custCode);
    }
    
    /**
     * 获取柜员密码
     */
    public String getCustPwd()
    {
        return getString(CUST_PWD);
    }
    
    /**
     * 设置柜员密码
     */
    public void setCustPwd(String custPwd)
    {
        set(CUST_PWD, custPwd);
    }
    
    /**
     * 获取机构代码
     */
    public String getJgdm()
    {
        return getString(JGDM);
    }
    
    /**
     * 设置机构代码
     */
    public void setJgdm(String jgdm)
    {
        set(JGDM, jgdm);
    }
    
    /**
     * 获取网点代码
     */
    public String getWddm()
    {
        return getString(WDDM);
    }
    
    /**
     * 设置网点代码
     */
    public void setWddm(String wddm)
    {
        set(WDDM, wddm);
    }
    
    /**
     * 获取创建时间
     */
    public Date getCreateDate()
    {
        return (Date) get(CREATE_DATE);
    }
    
    /**
     * 设置创建时间
     */
    public void setCreateDate(Date createDate)
    {
        Timestamp timestamp = new Timestamp(createDate.getTime());
        set(CREATE_DATE, timestamp);
    }
    
    /**
     * 获取更新时间
     */
    public Date getUpdateDate()
    {
      return (Date) get(UPDATE_DATE);
    }
    
    /**
     * 设置更新时间
     */
    public void setUpdateDate(Date updateDate)
    {
        Timestamp timestamp = new Timestamp(updateDate.getTime());
        set(UPDATE_DATE, timestamp);
    }
    
}
